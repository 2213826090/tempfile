<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_AUDIO_QUALITY_SWAP_CALL</UseCase>
    <Description>Check Audio Quality during GSM Mobile Originated Call on real network with a specified accessories</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <Parameters>
        <Parameter>
            <!-- DUT SIP address -->
            <Name>DUT_SIP_ADDRESS</Name>
            <Value>apxdut@192.168.0.150</Value>
        </Parameter>
        <Parameter>
            <!-- Peer SIP address -->
            <Name>PEER_SIP_ADDRESS</Name>
            <Value>apxrefphone@192.168.0.150</Value>
        </Parameter>
        <Parameter>
            <!-- DUT SIP profile -->
            <Name>DUT_SIP_PROFILE</Name>
            <Value>dut_sip_profile</Value>
        </Parameter>
        <Parameter>
            <!-- PEER SIP profile -->
            <Name>PEER_SIP_PROFILE</Name>
            <Value>ref_sip_profile</Value>
        </Parameter>
        <Parameter>
            <!-- Call sequence ([CSV|VOIP]) -->
            <Name>CALL_SEQUENCE</Name>
            <Value>[VOIP,VOIP]</Value>
        </Parameter>
        <Parameter>
            <!-- Call origin sequence(MO|MT) -->
            <Name>CALL_ORIGIN_TYPE_SEQUENCE</Name>
            <Value>[MO,MO]</Value>
        </Parameter>
        <Parameter>
            <!-- Call end type (NR|MR) -->
            <Name>CALL_END_TYPE_SEQUENCE</Name>
            <Value>[MR,MR]</Value>
        </Parameter>
        <Parameter>
            <!-- Accessorie type (EARPIECE|SPEAKER|HEADSET|BLUETOOTHA2DP|BLUETOOTHHSP) -->
            <Name>ACC_TYPE</Name>
            <Value>EARPIECE</Value>
        </Parameter>
        <Parameter>
             <!-- >Stream Call Volume in percent of dut phone -->
            <Name>CALL_VOLUME_DUT</Name>
            <Value>70</Value>
        </Parameter>
        <Parameter>
             <!-- >Stream Call Volume in percent of ref phone -->
            <Name>CALL_VOLUME_REF</Name>
            <Value>70</Value>
        </Parameter>
        <Parameter>
            <!-- Call type (2G|3G) -->
            <Name>CALL_TYPE</Name>
            <Value>3G</Value>
        </Parameter>
        <Parameter>
            <!-- number of swap -->
            <Name>SWAP_REPETITION</Name>
            <Value>2</Value>
        </Parameter>
        <Parameter>
            <!-- Signal tested direction (UL|DL) -->
            <Name>SIGNAL_TESTED_DIRECTION</Name>
            <Value>DL</Value>
        </Parameter>
        <Parameter>
            <!-- Call duration in seconds -->
            <Name>DURATION</Name>
            <Value>5</Value>
        </Parameter>
    </Parameters>
</TestCase>