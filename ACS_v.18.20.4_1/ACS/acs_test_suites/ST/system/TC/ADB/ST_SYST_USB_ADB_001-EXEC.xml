      <Name>RUN_TIMEOUT</Name>
      <Value>1</Value>
    </Parameter>
	<Parameter>
	  <Name>RUN_EXPECT_RESULT</Name>
	  <Value>Hello</Value>
	</Parameter>
    	
    <Parameter>
      <Name>TEAR_DOWN_CMD</Name>
      <Value>adb shell echo "tear down"</Value>
    </Parameter>
    <Parameter>
      <Name>TEAR_DOWN_TIMEOUT</Name>
      <Value>1</Value>
    </Parameter>
	<Parameter>
	  <Name>TEAR_DOWN_EXPECT_RESULT</Name>
	  <Value>0</Value>
	</Parameter>
  </Parameters>  
