      <Name>SETUP_CMD</Name>
      <Value>adb shell echo "setup"</Value>
    </Parameter>
    <Parameter>
      <Name>SETUP_TIMEOUT</Name>
      <Value>1</Value>
    </Parameter>
	<Parameter>
	  <Name>SETUP_EXPECT_RESULT</Name>
