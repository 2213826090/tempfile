<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_MOBILITY_LTE_SMS</UseCase>
  <Description>TEL - This UseCase valid SMS on LTE over SG</Description>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <Parameters>
    <!-- SMS PARAMETERS -->
    <Parameter>
        <!-- SMS DIRECTION (MO [Mobile Originated] |MT [Mobile Terminated]) -->
        <Name>SMS_DIRECTION</Name>
        <Value>MT</Value>
    </Parameter>
    <Parameter>
        <!-- SMS TEXT Sent or Received  -->
        <Name>SMS_TEXT</Name>
        <Value>This is a short MT SMS</Value>
    </Parameter>
    <Parameter>
        <!-- Phone number of SMS Receiver or Sender. If value is [PHONE_NUMBER] the value used will be the phoneNumber defined in the Phone_Catalog.xml -->
        <Name>PHONE_NUMBER</Name>
        <Value>0622219252</Value>
    </Parameter>
    <Parameter>
        <!-- SMS transfer timeout in seconds -->
        <Name>SMS_TRANSFER_TIMEOUT</Name>
        <Value>15</Value>
    </Parameter>
    <Parameter>
        <!-- SMS Data coding scheme in hexadecimal -->
        <Name>DATA_CODING_SCHEME</Name>
        <Value>00</Value>
    </Parameter>
    <!-- LTE CELL PARAMETERS -->
    <Parameter>
        <!-- Multiple-Input Multiple-Output to set : true or false -->
        <Name>LTE_MIMO</Name>
        <Value>FALSE</Value>
    </Parameter>
    <Parameter>
        <!-- Operation Signal mode to set : BSE or SA -->
        <Name>LTE_SIGNAL_MODE</Name>
        <Value>BSE</Value>
    </Parameter>
    <Parameter>
        <!-- Physical Cell ID : 0 to 503 -->
        <Name>LTE_PHYSICAL_CELL_ID</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Cell Setup : "A" for A-cell or "B" for B-cell -->
        <Name>LTE_CELL_ID</Name>
        <Value>A</Value>
    </Parameter>
    <Parameter>
        <!-- Cell power for RFO1 -->
        <Name>LTE_CELL_POWER_RFO1</Name>
        <Value>-60</Value>
    </Parameter>
    <Parameter>
        <!-- Cell power for RFO2 -->
        <Name>LTE_CELL_POWER_RFO2</Name>
        <Value>-60</Value>
    </Parameter>
    <Parameter>
        <!-- Mobile Country Code 0 to 999 -->
        <Name>LTE_MCC</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Mobile Network Code 0 to 999 -->
        <Name>LTE_MNC</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Cell Band (from Band 1 to Band 32) -->
        <Name>LTE_CELL_BAND</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Downlink channel -->
        <Name>LTE_DL_EARFCN</Name>
        <Value>300</Value>
    </Parameter>
    <Parameter>
        <!-- Scenario filename and path to load on PXT -->
        <Name>LTE_SCENARIO_PATH</Name>
        <Value>UC_default_bearer_IratRedirectionWCDMASuppServices_NewQoS_Target10700.LBMF</Value>
    </Parameter>
    <!-- 3GSM CELL PARAMETERS -->
    <Parameter>
        <!-- 3GSM Cell Technology (2G|3G) -->
        <Name>3GSM_CELL_TECH</Name>
        <Value>2G</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell Band
            For 2G : EGSM|GSM850|PCS|DCS
            For 3G : 1,2,5,8
        -->
        <Name>3GSM_CELL_BAND</Name>
        <Value>EGSM</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Downlink channel -->
        <Name>3GSM_DL_ARFCN</Name>
        <Value>20</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell Service ( (GSM|GPRS|EGPRS) for 2G CELL, (CIRCUIT|PACKET|CIRCUIT_PACKET) for 3G CELL) -->
        <Name>3GSM_CELL_SERVICE</Name>
        <Value>EGPRS</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM cell power -->
        <Name>3GSM_CELL_POWER</Name>
        <Value>-70</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell LAC, For 2G and 3G: 0 to 65535-->
        <Name>3GSM_LAC</Name>
        <Value>10</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell Routing Area, For 2G and 3G: 1 to 255-->
        <Name>3GSM_RAC</Name>
        <Value>25</Value>
    </Parameter>
  </Parameters>
</TestCase>
