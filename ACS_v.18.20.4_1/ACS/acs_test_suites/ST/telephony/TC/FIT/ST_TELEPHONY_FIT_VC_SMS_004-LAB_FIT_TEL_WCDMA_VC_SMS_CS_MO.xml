<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_FIT_TEL_WCDMA_VC_SMS_CS_MO</UseCase>
  <Description>Test send of SMS Cs during a WCDMA voice call</Description>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <Parameters>
    <Parameter>
      <!-- Cell Band (from Band 1 to Band 8)-->
      <Name>CELL_BAND</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- Downlink UARFCN Frequency -->
      <Name>DL_UARFCN</Name>
      <Value>10700</Value>
    </Parameter>
    <Parameter>
      <!--Cell Service (CIRCUIT | PACKET | CIRCUIT_PACKET | RBTEST) -->
      <Name>CELL_SERVICE</Name>
      <Value>CIRCUIT</Value>
    </Parameter>
    <Parameter>
      <!-- Dialled Phone number (MSISDN). If value is [PHONE_NUMBER] the value used will be the phoneNumber defined in the Phone_Catalog.xml -->
      <Name>PHONE_NUMBER</Name>
      <Value>0616915832</Value>
    </Parameter>
    <Parameter>
      <!-- SMS text to send -->
      <Name>SMS_TEXT</Name>
      <Value>This is my SMS text !</Value>
    </Parameter>
    <Parameter>
      <!-- SMS transfer timeout in seconds -->
      <Name>SMS_TRANSFER_TIMEOUT</Name>
      <Value>10</Value>
    </Parameter>
  </Parameters>
</TestCase>
