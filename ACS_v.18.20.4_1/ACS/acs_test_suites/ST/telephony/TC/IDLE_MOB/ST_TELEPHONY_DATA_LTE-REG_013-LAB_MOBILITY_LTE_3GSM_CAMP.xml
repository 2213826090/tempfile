<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_MOBILITY_LTE_3GSM_CAMP</UseCase>
  <Description>LTE Nw Registration - E-UTRA Band IX 1800</Description>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <Parameters>
    <!-- LTE CELL PARAMETERS -->
    <Parameter>
        <!-- Multiple-Input Multiple-Output to set : true or false -->
        <Name>LTE_MIMO</Name>
        <Value>FALSE</Value>
    </Parameter>
    <Parameter>
        <!-- Operation Signal mode to set : BSE or SA -->
        <Name>LTE_SIGNAL_MODE</Name>
        <Value>BSE</Value>
    </Parameter>
    <Parameter>
        <!-- Physical Cell ID : 0 to 503 -->
        <Name>LTE_PHYSICAL_CELL_ID</Name>
        <Value>0</Value>
    </Parameter>
    <Parameter>
        <!-- Cell Setup : "A" for A-cell or "B" for B-cell -->
        <Name>LTE_CELL_ID</Name>
        <Value>A</Value>
    </Parameter>
    <Parameter>
        <!-- Cell power for RFO1 -->
        <Name>LTE_CELL_POWER_RFO1</Name>
        <Value>-60</Value>
    </Parameter>
    <Parameter>
        <!-- Cell power for RFO2 -->
        <Name>LTE_CELL_POWER_RFO2</Name>
        <Value>-60</Value>
    </Parameter>
    <Parameter>
        <!-- Mobile Country Code 0 to 999 -->
        <Name>LTE_MCC</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Mobile Network Code 0 to 999 -->
        <Name>LTE_MNC</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Cell Band (from Band 1 to Band 32) -->
        <Name>LTE_CELL_BAND</Name>
        <Value>9</Value>
    </Parameter>
    <Parameter>
        <!-- Downlink channel -->
        <Name>LTE_DL_EARFCN</Name>
        <Value>3975</Value>
    </Parameter>
    <Parameter>
        <!-- Scenario filename and path to load on PXT -->
        <Name>LTE_SCENARIO_PATH</Name>
        <Value>UC_default_bearer_IratRedirectionWCDMASuppServices_NewQoS_Target10700_LAC.LBMF</Value>
    </Parameter>
    <!-- 3GSM CELL PARAMETERS -->
    <Parameter>
        <!-- 3GSM Cell Technology (2G|3G) -->
        <Name>3GSM_CELL_TECH</Name>
        <Value>3G</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell Band
            For 2G : EGSM|GSM850|PCS|DCS
            For 3G : 1,2,5,8
        -->
        <Name>3GSM_CELL_BAND</Name>
        <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Downlink channel -->
        <Name>3GSM_DL_ARFCN</Name>
        <Value>10700</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell Service ( (GSM|GPRS|EGPRS) for 2G CELL, (CIRCUIT|PACKET|CIRCUIT_PACKET) for 3G CELL) -->
        <Name>3GSM_CELL_SERVICE</Name>
        <Value>PACKET</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM cell power -->
        <Name>3GSM_CELL_POWER</Name>
        <Value>-80</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell LAC, For 2G and 3G: 0 to 65535-->
        <Name>3GSM_LAC</Name>
        <Value>458</Value>
    </Parameter>
    <Parameter>
        <!-- 3GSM Cell Routing Area, For 2G and 3G: 1 to 255-->
        <Name>3GSM_RAC</Name>
        <Value>25</Value>
    </Parameter>
    <!-- PING PARAMETERS -->
    <Parameter>
        <!-- Number of Packet to be sent -->
        <Name>PACKET_COUNT</Name>
        <Value>10</Value>
    </Parameter>
    <Parameter>
        <!-- Size of a data packet (in bytes) sent when trying to ping the network -->
        <Name>PACKET_SIZE</Name>
        <Value>500</Value>
    </Parameter>
    <Parameter>
        <!-- Target of Ping Packet Loss Rate (in %) -->
        <Name>TARGET_PACKET_LOSS_RATE</Name>
        <Value>0</Value>
    </Parameter>
  </Parameters>
</TestCase>