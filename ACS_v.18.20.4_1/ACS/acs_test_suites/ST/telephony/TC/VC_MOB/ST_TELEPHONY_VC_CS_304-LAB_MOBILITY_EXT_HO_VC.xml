<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_MOBILITY_EXT_HO_VC</UseCase>
  <Description> Perform several 3G/2G Handovers during Voice Call </Description>
  <Requirements>
    <Contour>
        <Global_ID>191292</Global_ID>
        <Test_Case_ID>ST_COM_VC_CS_306</Test_Case_ID>
        <Name>Voice Call Handover - interRAT 3G to 2G</Name>
    </Contour>
  </Requirements>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <Parameters>
    <Parameter>
      <!-- NS1 Cell Technology (2G|3G) -->
      <Name>NS1_CELL_TECH</Name>
      <Value>3G</Value>
    </Parameter>
    <Parameter>
      <!-- NS1 Cell Band
         For 2G : EGSM|GSM850|PCS|DCS
         For 3G : 1,2,5,8
      -->
      <Name>NS1_CELL_BAND</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- NS2 Cell Technology (2G|3G) -->
      <Name>NS2_CELL_TECH</Name>
      <Value>2G</Value>
    </Parameter>
    <Parameter>
      <!-- NS2 Cell Band
         For 2G : EGSM|GSM850|PCS|DCS
         For 3G : 1,2,5,8
      -->
      <Name>NS2_CELL_BAND</Name>
      <Value>DCS</Value>
    </Parameter>
    <Parameter>
      <!-- CELL HO NUMBER (Go and return) -->
      <Name>CELL_HO_NUMBER</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
        <!-- Dialled Phone number (MSISDN). If value is [PHONE_NUMBER] the value used will be the phoneNumber defined in the Phone_Catalog.xml -->
        <Name>PHONE_NUMBER</Name>
        <Value>0622219252</Value>
    </Parameter>
  </Parameters>
</TestCase>