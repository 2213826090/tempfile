<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_LTE_REG</UseCase>
    <Description>Registration over LTE simulated network and then Ping</Description>
    <b2bIteration>100</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- Multiple-Input Multiple-Output to set : true or false -->
            <Name>MIMO</Name>
            <Value>FALSE</Value>
        </Parameter>
        <Parameter>
            <!-- Duplex type TDD or FDD -->
            <Name>DUPLEX_TYPE</Name>
            <Value>TDD</Value>
        </Parameter>
        <Parameter>
            <!-- Select band : 1 or 2  -->
            <Name>CELL_BAND</Name>
            <Value>38</Value>
        </Parameter>
        <Parameter>
            <!-- Downlink channel -->
            <Name>DL_EARFCN</Name>
            <Value>38000</Value>
         </Parameter>
        <Parameter>
            <!-- number of antennas to set : 1 or 2  -->
            <Name>ANTENNAS_NUMBER</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Number of Packet to be sent -->
            <Name>PACKET_COUNT</Name>
            <Value>10</Value>
        </Parameter>
        <Parameter>
            <!-- Size of a data packet (in bytes) sent when trying to ping the network -->
            <Name>PACKET_SIZE</Name>
            <Value>32</Value>
        </Parameter>
        <Parameter>
            <!-- Target of Ping Packet Loss Rate (in %) -->
            <Name>TARGET_PACKET_LOSS_RATE</Name>
            <Value>0</Value>
        </Parameter>
    </Parameters>
</TestCase>