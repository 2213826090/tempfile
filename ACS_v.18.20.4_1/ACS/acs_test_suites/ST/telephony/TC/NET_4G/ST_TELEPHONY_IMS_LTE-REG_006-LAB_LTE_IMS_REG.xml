<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_LTE_IMS_REG</UseCase>
    <Description>Registration over LTE simulated network and then Ping</Description>
    <Requirements>
        <Contour>
            <Global_ID>16393740</Global_ID>
            <Test_Case_ID>ST_TELEPHONY_IMS_LTE-REG_006</Test_Case_ID>
            <Name>IMS over LTE - B2B Registration and Deregistration over LTE (x100)</Name>
        </Contour>
    </Requirements>
    <b2bIteration>100</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <Name>IMS</Name>
            <Value>ON</Value>
        </Parameter>
        <Parameter>
            <!-- Multiple-Input Multiple-Output to set : true or false -->
            <Name>MIMO</Name>
            <Value>FALSE</Value>
        </Parameter>
        <Parameter>
            <!-- number of antennas to set : 1 or 2  -->
            <Name>ANTENNAS_NUMBER</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!--  TRANSMISSION_MODE to set: TM1, TM2, TM3,TM4 ... -->
            <Name>TRANSMISSION_MODE</Name>
            <Value>TM1</Value>
        </Parameter>
        <Parameter>
            <!-- DL RB_SIZE to set:
            1.4 MHz = 1 to 6
            3 MHz = 1 to 15
            5 MHz = 1 to 25
            10 MHz = 1 to 50
            15 MHz = 1 to 75
            20 MHz = 1 to 100 -->
            <Name>DL_RB_SIZE</Name>
            <Value>50</Value>
        </Parameter>
        <Parameter>
            <!-- UL RB_SIZE to set:
            1.4 MHz = 1 to 6
            3 MHz = 1 to 15
            5 MHz = 1 to 25
            10 MHz = 1 to 50
            15 MHz = 1 to 75
            20 MHz = 1 to 100 -->
            <Name>UL_RB_SIZE</Name>
            <Value>50</Value>
        </Parameter>
        <Parameter>
            <!-- TYPE0_BITMAP to set: requires to set TM3 or TM4 profile, MIMO to be set in scenario file
            1.4 MHz = 1 to 63 (0x3F)
            3 MHz = 1 to 255 (0xFF)
            5 MHz = 1 to 8191 (0x1FFF)
            10 MHz = 1 to 131071 (0x1FFFF)
            15 MHz = 1 to 524287 (0x7FFFF)
            20 MHz = 1 to 33554431 (0x1FFFFFF) -->
            <Name>TYPE0_BITMAP</Name>
            <Value>63</Value>
        </Parameter>
        <Parameter>
            <!-- UL GRANT MODE to set: AUTO or FIXEDMAC -->
            <Name>UL_GRANT_MODE</Name>
            <Value>FIXEDMAC</Value>
        </Parameter>
        <Parameter>
            <!-- DL_I_MCS to set: 0 to 28 -->
            <Name>DL_I_MCS</Name>
            <Value>25</Value>
        </Parameter>
        <Parameter>
            <!-- UL_I_MCS to set: 0 to 31 -->
            <Name>UL_I_MCS</Name>
            <Value>3</Value>
        </Parameter>
        <Parameter>
            <!-- Operation mode to set : BSE or SA -->
            <Name>SIGNAL_MODE</Name>
            <Value>BSE</Value>
        </Parameter>
        <Parameter>
            <!-- Physical Cell ID : 0 to 503 -->
            <Name>PHYSICAL_CELL_ID</Name>
            <Value>0</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Setup : "A" for A-cell or "B" for B-cell -->
            <Name>CELL_ID</Name>
            <Value>A</Value>
        </Parameter>
        <Parameter>
            <!-- Cell power for RFO1 -->
            <Name>CELL_POWER_RFO1</Name>
            <Value>-40</Value>
        </Parameter>
        <Parameter>
            <!-- Cell power for RFO2 -->
            <Name>CELL_POWER_RFO2</Name>
            <Value>-40</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Country Code 0 to 999 -->
            <Name>MCC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Network Code 0 to 999 -->
            <Name>MNC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Band (from Band 1 to Band 32) -->
            <Name>CELL_BAND</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Downlink channel -->
            <Name>DL_EARFCN</Name>
            <Value>300</Value>
        </Parameter>
        <Parameter>
            <!-- Scenario filename and path to load on PXT -->
            <Name>SCENARIO_PATH</Name>
            <Value>INTEL_B1_10M_TM1_Default_CSFBGSMv2b_ESMstatusfix.LBMF</Value>
        </Parameter>
    </Parameters>
</TestCase>
