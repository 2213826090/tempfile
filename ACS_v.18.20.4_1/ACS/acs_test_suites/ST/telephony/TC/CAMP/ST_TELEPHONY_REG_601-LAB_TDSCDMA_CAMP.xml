<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_TDSCDMA_CAMP</UseCase>
  <Description>TD-SCDMA Network Registration on simulated network</Description>
  <b2bIteration>100</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <Parameters>
    <Parameter>
      <!-- Cell Band -->
      <Name>CELL_BAND</Name>
      <Value>34</Value>
    </Parameter>
    <Parameter>
      <!-- Cell power in dBm -->
      <Name>CELL_POWER</Name>
      <Value>-80</Value>
    </Parameter>
    <Parameter>
      <!-- Mode used to switch on/off -->
      <Name>SWITCH_MODE</Name>
      <Value>airplane</Value>
    </Parameter>
    <Parameter>
      <!-- Mode used to switch on/off -->
      <Name>PDP_ACTIVATION</Name>
      <Value>True</Value>
    </Parameter>
  </Parameters>
</TestCase>