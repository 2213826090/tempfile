<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_WCDMA_CAMP</UseCase>
  <Description>Power on/off WCDMA Mobile Camp on simulated network</Description>
  <Requirements>
      <Contour>
          <Global_ID>16566839</Global_ID>
          <Test_Case_ID>ST_TELEPHONY_REG_012</Test_Case_ID>
          <Name>NetworkRegistration - 3GCamp - Band VII (2600)</Name>
      </Contour>
  </Requirements>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <TcExpectedResult>PASS</TcExpectedResult>
  <Parameters>
    <Parameter>
      <!-- Switch off/on Mode -->
      <Name>SWITCH_MODE</Name>
      <Value>softshutdown</Value>
    </Parameter>
    <Parameter>
      <!-- Cell Band -->
      <Name>CELL_BAND</Name>
      <Value>7</Value>
    </Parameter>
    <Parameter>
      <!-- DL UARFCN -->
      <Name>DL_UARFCN</Name>
      <Value>2400</Value>
    </Parameter>
    <Parameter>
      <!-- Cell Service CIRCUIT|PACKET|CIRCUIT_PACKET-->
      <Name>CELL_SERVICE</Name>
      <Value>PACKET</Value>
    </Parameter>
    <Parameter>
      <!-- Cell power in dBm -->
      <Name>CELL_POWER</Name>
      <Value>-80</Value>
    </Parameter>
    <Parameter>
      <!-- Mobile Country Code 0 to 999-->
      <Name>MCC</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- Mobile Network Code 0 to 999 -->
      <Name>MNC</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- PDP ACTIVATION TRUE|FALSE -->
      <Name>PDP_ACTIVATION</Name>
      <Value>FALSE</Value>
    </Parameter>
  </Parameters>
</TestCase>
