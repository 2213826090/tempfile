<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_WCDMA_CAMP</UseCase>
  <Description>WCDMA Mobile Camp on simulated network</Description>
  <b2bIteration>200</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode> 
  <Parameters>
    <Parameter>
      <!-- Cell Band -->
      <Name>CELL_BAND</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- DL UARFCN -->
      <Name>DL_UARFCN</Name>
      <Value>10700</Value>
    </Parameter>
    <Parameter>
      <!-- Cell Service CIRCUIT|PACKET|CIRCUIT_PACKET-->
      <Name>CELL_SERVICE</Name>
      <Value>PACKET</Value>
    </Parameter>
    <Parameter>
      <!-- Cell power in dBm -->
      <Name>CELL_POWER</Name>
      <Value>-70</Value>
    </Parameter>
    <Parameter>
      <!-- Mobile Country Code 0 to 999-->
      <Name>MCC</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- Mobile Network Code 0 to 999 -->
      <Name>MNC</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- PDP ACTIVATION TRUE|FALSE -->
      <Name>PDP_ACTIVATION</Name>
      <Value>TRUE</Value>
    </Parameter>
  </Parameters>
</TestCase>
