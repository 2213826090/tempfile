<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_MOBILITY_EXT_HO_VC</UseCase>
  <Description> Perform several 2G/3G Handovers during Voice Call </Description>
  <b2bIteration>1000</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>  
  <Parameters>
    <Parameter>
      <!-- Handover number to perform -->
      <Name>CELL_JUMP_NUMBER</Name>
      <Value>2</Value> <!-- will then perform a ping pong 2G -> 3G -> 2G -->
    </Parameter>
    <Parameter>
      <!-- CONFIGURATION_FILE parameter representing a xml file name 
      (without extension) in which equipments configuration is described.
      Configuration files must be stored in _Configs/EquipmentConfiguration directory. -->
      <Name>CONFIGURATION_FILE</Name>
      <Value>ST\telephony\Configurations\CONF_ST_COM_VC_CS_308-LAB_MOBILITY_EXT_HO_VC</Value>
    </Parameter>
    <Parameter>
        <!-- Dialled Phone number (MSISDN). If value is [PHONE_NUMBER] the value used will be the phoneNumber defined in the Phone_Catalog.xml -->
        <Name>PHONE_NUMBER</Name>
        <Value>0622219252</Value>
    </Parameter>
  </Parameters>
</TestCase>