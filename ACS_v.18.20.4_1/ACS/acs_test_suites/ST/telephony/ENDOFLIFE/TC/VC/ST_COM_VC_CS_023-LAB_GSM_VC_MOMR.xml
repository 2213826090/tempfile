<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <UseCase>LAB_GSM_VC_MOMR</UseCase>
  <Description>GSM Mobile Originated Call / Mobile Release on simulated network</Description>
  <b2bIteration>200</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode>
  <Parameters>
    <Parameter>
        <!-- Cell Band (PGSM|RGSM|EGSM|GSM450|GSM480|GSM750|GSM850|TGSM810|PCS|DCS) -->
      <Name>CELL_BAND</Name>
      <Value>DCS</Value>
    </Parameter>
    <Parameter>
        <!-- Broadcast Channel Arfcn frequency -->
      <Name>BCH_ARFCN</Name>
      <Value>698</Value>
    </Parameter>
    <Parameter>
        <!-- Traffic Channel Arfcn frequency -->
        <Name>TCH_ARFCN</Name>
        <Value>650</Value>
    </Parameter>
    <Parameter>
        <!-- Cell Service (GSM|GPRS|EGPRS) -->
        <Name>CELL_SERVICE</Name>
        <Value>EGPRS</Value>
    </Parameter>
    <Parameter>
        <!-- Cell Power (in dBm) -->
        <Name>CELL_POWER</Name>
        <Value>-80</Value>
    </Parameter>
    <Parameter>
        <!-- Voice coder rate 
          Possible values:
             FR
             EFR
             HR
             FR_AMR_NB_[1220|1020|795|740|670|590|515|475]
             HR_AMR_NB_[795|740|670|590|515|475]
             AMR_WB_[1265|885|660]
        -->
        <Name>VOICE_CODER_RATE</Name>
        <Value>FR</Value>
    </Parameter>
    <Parameter>
        <!-- Dialled Phone number (MSISDN). If value is [PHONE_NUMBER] the value used will be the phoneNumber defined in the Phone_Catalog.xml -->
        <Name>PHONE_NUMBER</Name>
        <Value>0493001498</Value>
    </Parameter>
    <Parameter>
        <!-- Call Duration (in seconds) -->
        <Name>CALL_DURATION</Name>
        <Value>10</Value>
    </Parameter>
    <Parameter>
      <!-- Mobile Country Code 0 to 999-->
      <Name>MCC</Name>
      <Value>1</Value>
    </Parameter>
    <Parameter>
      <!-- Mobile Network Code 0 to 999 -->
      <Name>MNC</Name>
      <Value>1</Value>
    </Parameter>
  </Parameters>
</TestCase>
