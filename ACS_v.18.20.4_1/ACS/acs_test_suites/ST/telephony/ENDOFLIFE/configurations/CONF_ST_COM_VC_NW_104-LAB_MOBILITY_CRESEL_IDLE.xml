	<GPIBCommand equipment="NETWORK_SIMULATOR1" command="CALL:SCOD 256" description="Set the scrambling code"/>
	<GPIBCommand equipment="NETWORK_SIMULATOR1" command="CALL:UTRAN:FDDUPLEX:TABLE 10700,1,255,1" description="Enable the 3G neighbour Cell 1 . Set its Uarfcn, Scrambling code."/>
	
