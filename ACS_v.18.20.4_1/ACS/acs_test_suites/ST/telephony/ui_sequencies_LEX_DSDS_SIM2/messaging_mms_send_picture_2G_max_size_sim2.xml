<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<UI_SEQ xsi:noNamespaceSchemaLocation="ACS_ANDROID_UI_CHECK.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>  
   <ACTION>
    <CMD>exec</CMD>
    <PARAMETERS>start -a android.intent.action.Main -n com.android.phone/.Settings</PARAMETERS>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
   <ACTION>
    <CMD>settings_toggle_preferred_radio_2G_sim2</CMD>
    <TIMEOUT>1</TIMEOUT>
    <TRIG_LOG>set preferred network type done</TRIG_LOG>
  </ACTION>
  <ACTION>
    <CMD>keypress</CMD>
    <PARAMETERS>KEYCODE_BACK</PARAMETERS>
  </ACTION>
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>  
  <ACTION>
    <CMD>open_sms_menu</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
  <!-- sending a SMS of 160 characters-->
  <ACTION>
    <CMD>mms_send_picture_sim2</CMD>
    <PARAMETERS>0033629059146;MMS WITH PICTURE 2G MAX SIZE;aaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccdddddddddd</PARAMETERS>
    <TIMEOUT>1</TIMEOUT>
        <!--<TRIG_LOG>com.android.mms/.ui.ComposeMessageActivity</TRIG_LOG>-->
    <!--<TRIG_LOG>SETUP_DATA_CALL</TRIG_LOG>-->
    <!--<TRIG_LOG>[INITING,CONNECTED]</TRIG_LOG>-->
	<TRIG_LOG>set reason as dataDisabled, for type mms,current state CONNECTED_-_SETUP_DATA_CALL</TRIG_LOG>
  </ACTION>
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
  <ACTION>
    <CMD>open_sms_menu</CMD>
    <TIMEOUT>1</TIMEOUT>
    <TRIG_LOG></TRIG_LOG>
  </ACTION>  
  <ACTION>
    <CMD>sms_clear_all_sms</CMD>
    <TIMEOUT>1</TIMEOUT>
    <TRIG_LOG></TRIG_LOG>
  </ACTION>
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>   
   <ACTION>
    <CMD>exec</CMD>
    <PARAMETERS>start -a android.intent.action.Main -n com.android.phone/.Settings</PARAMETERS>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
   <ACTION>
    <CMD>settings_toggle_preferred_radio_2G_sim2</CMD>
    <TIMEOUT>1</TIMEOUT>
    <TRIG_LOG>set preferred network type done</TRIG_LOG>
  </ACTION>
  <ACTION>
    <CMD>keypress</CMD>
    <PARAMETERS>KEYCODE_BACK</PARAMETERS>
  </ACTION>
</UI_SEQ>