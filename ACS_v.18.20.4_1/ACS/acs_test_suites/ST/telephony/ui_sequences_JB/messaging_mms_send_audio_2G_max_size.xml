<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<UI_SEQ xsi:noNamespaceSchemaLocation="ACS_ANDROID_UI_CHECK.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- every SMS must be cleared deleted before starting the test-->
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>  
  <ACTION>
    <CMD>exec</CMD>
    <PARAMETERS>start -a android.intent.action.Main -n com.android.phone/.Settings</PARAMETERS>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
  <ACTION>
    <CMD>settings_toggle_preferred_radio_2G</CMD>
    <TRIG_LOG>Use2GOnlyCheckBoxPreference: set preferred network</TRIG_LOG>
    <TIMEOUT>10</TIMEOUT>
  </ACTION>
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>  
  <ACTION>
    <CMD>open_sms_menu</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
  <!-- sending a SMS of 160 characters-->
  <ACTION>
    <CMD>mms_send_audio</CMD>
    <PARAMETERS>0033629059146;MMS WITH AUDIO 2G MAX SIZE;aaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccaaaaaaaaaabbbbbbbbbbccccccccccdddddddddd;ST_MMS_023</PARAMETERS>
    <TIMEOUT>1</TIMEOUT>
    <!--<TRIG_LOG>com.android.mms/.ui.ComposeMessageActivity</TRIG_LOG>-->
    <!--<TRIG_LOG>SETUP_DATA_CALL</TRIG_LOG>-->
    <!--<TRIG_LOG>[INITING,CONNECTED]</TRIG_LOG>-->
    <!--<TRIG_LOG>set reason as dataDisabled, for type mms,current state CONNECTED_-_SETUP_DATA_CALL</TRIG_LOG>-->
	<TRIG_LOG>ConnectivityChange for mobile_mms: CONNECTED/CONNECTED_-_> SETUP_DATA_CALL</TRIG_LOG>
  </ACTION>
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
  <ACTION>
    <CMD>open_sms_menu</CMD>
    <TIMEOUT>1</TIMEOUT>
    <TRIG_LOG></TRIG_LOG>
  </ACTION>  
  <ACTION>
    <CMD>sms_clear_all_sms</CMD>
    <TIMEOUT>1</TIMEOUT>
    <TRIG_LOG></TRIG_LOG>
  </ACTION>
  <ACTION>
    <!--Go back to home page-->
    <CMD>go_home</CMD>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>  
  <ACTION>
    <CMD>exec</CMD>
    <PARAMETERS>start -a android.intent.action.Main -n com.android.phone/.Settings</PARAMETERS>
    <TIMEOUT>1</TIMEOUT>
  </ACTION>
  <ACTION>
    <CMD>settings_toggle_preferred_radio_2G</CMD>
    <TRIG_LOG>Use2GOnlyCheckBoxPreference: set preferred network</TRIG_LOG>
    <TIMEOUT>10</TIMEOUT>
  </ACTION>
</UI_SEQ>