<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>TEST_STEP_ENGINE</UseCase>
    <Description>Power consumption measurement (in mA) in LTE standby with WLAN associated</Description>
    <b2bIteration>3</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>3</TcMaxAttempt>
    <SavePatRawData>True</SavePatRawData>
    <PowerCalculation>False</PowerCalculation>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <Name>SAVE_POWER_DATA</Name>
            <Value>True</Value>
        </Parameter>
        <Parameter>
            <Name>CELL_BAND</Name>
            <Value>PWR_4G_IDLE</Value>
        </Parameter>
        <Parameter>
            <Name>RAT</Name>
            <Value>LTE</Value>
        </Parameter>
        <Parameter>
            <Name>MODE</Name>
            <Value>s3</Value>
        </Parameter>
        <Parameter>
            <!-- Sleep duration before making the measure in seconds -->
            <Name>SLEEP_DURATION</Name>
            <Value>1800</Value>
        </Parameter>
        <Parameter>
            <!-- Sleep duration before making the measure in seconds -->
            <Name>TEST_DURATION</Name>
            <Value>1800</Value>
        </Parameter>
        <Parameter>
            <!--
                 Configuration for Sysdebug modules.
                 Syntax is :
                 module_name[: [param1[=value1]][, param2[=value2]]*];
              -->
             <Name>SYSDEBUG_MODULES</Name>
             <Value>
                Residency: s3,s0i3,s0i1,lpmp3;
                S3Failure;
                ActiveWakeupSource;
                WakeLocks;
                Alarms;
                IrqWakeups;
                ModemPanic;
                CrashInfo;
                Thermals;
                Dstates;
                Interrupts;
             </Value>
         </Parameter>
        <Parameter>
            <!-- config type for agilent -->
            <Name>CONFIG_TYPE</Name>
            <Value>IDLE</Value>
        </Parameter>
        <Parameter>
            <Name>TEST_STEP_ENGINE_ENTRY</Name>
            <Value>ST/pnp/UC/StandbyCmw500_wifi.xml</Value>
        </Parameter>
    </Parameters>
</TestCase>
