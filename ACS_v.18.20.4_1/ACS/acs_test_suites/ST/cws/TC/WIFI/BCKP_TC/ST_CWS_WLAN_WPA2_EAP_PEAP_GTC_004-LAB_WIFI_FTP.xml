<TestCase>
  <UseCase>LAB_WIFI_FTP</UseCase>
  <Description>FTP Upload / Download over WLAN</Description>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>True</b2bContinuousMode> 
  <TcMaxAttempt>2</TcMaxAttempt>
  <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
  <Parameters>
    <Parameter>
      <Name>WIFI_STANDARD</Name>
      <Value>gb</Value>
    </Parameter>
    <Parameter>
      <Name>WIFI_SECURITY</Name>
      <Value>EAP-WPA</Value>
    </Parameter>
    <Parameter>
      <Name>EAP_METHOD</Name>
      <Value>PEAP</Value>
    </Parameter>
    <Parameter>
      <Name>PHASE2_AUTH</Name>
      <Value>GTC</Value>
    </Parameter>

    <Parameter>
      <Name>WIFI_PASSPHRASE</Name>
      <Value>NONE</Value>
    </Parameter>
    <Parameter>
      <Name>IP_SETTING_ENABLE</Name>
      <Value>False</Value>
    </Parameter> 
    <Parameter>
      <Name>IP_ADDRESS</Name>
      <Value>192.168.0.149</Value>
    </Parameter> 
    <Parameter>
      <Name>NETMASK</Name>
      <Value>255.255.255.0</Value>
    </Parameter> 
    <Parameter>
      <Name>GATEWAY</Name>
      <Value>192.168.0.1</Value>
    </Parameter> 
    <Parameter>
      <Name>DNS1</Name>
      <Value>192.168.0.5</Value>
    </Parameter> 
    <Parameter>
      <Name>DNS2</Name>
      <Value>0.0.0.0</Value>
    </Parameter>

    <Parameter>
      <Name>DIRECTION</Name>
      <Value>DL</Value>
    </Parameter>
    <Parameter>
      <Name>DL_FILE</Name>
      <Value>pub/get10M</Value>
    </Parameter>
    <Parameter>
      <Name>UL_FILE</Name>
      <Value>10MB.txt</Value>
    </Parameter>
    <Parameter>
      <Name>XFER_TIMEOUT</Name>
      <Value>120</Value>
    </Parameter>
  </Parameters>
</TestCase>