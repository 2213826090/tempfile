<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_NFC_HOST_READER_MODE</UseCase>
    <Description>Write data in tag, read it and compare to check if each action succeeds</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>false</b2bContinuousMode>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <Parameters>
        <Parameter>
            <!-- If 'YES', the robot move will take into account the antenna position on the DUT as an offset -->
            <Name>USE_ANTENNA_OFFSET</Name>
            <Value>YES</Value>
        </Parameter>
        <Parameter>
            <!-- Define the tag type de write and read -->
            <Name>TAG_TYPE</Name>
            <Value>TYPE4_B</Value>
        </Parameter>
        <Parameter>
            <!-- Define the RTD -->
            <Name>RTD_TYPE</Name>
            <Value>RTD_TEXT</Value>
        </Parameter>
        <Parameter>
            <!-- Define the data to write -->
            <Name>DATA</Name>
            <Value>RANDOM</Value>
        </Parameter>
    </Parameters>
</TestCase>
