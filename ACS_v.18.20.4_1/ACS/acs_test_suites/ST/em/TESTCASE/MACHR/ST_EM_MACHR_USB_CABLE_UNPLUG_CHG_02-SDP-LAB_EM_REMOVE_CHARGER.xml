<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_REMOVE_CHARGER</UseCase>
    <Description>UnPlug USB SDP charger while platform is ON: Check the fuel gauging register when the Wall charger SDP is unplugged</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcMaxAttempt>1</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- This the Target to test. The target name and values are listed in the EnergyManagement.xml target file -->
            <Name>EM_TARGET</Name>
            <Value>MSIC_REGISTER2.BATTERY.STATUS;MSIC_REGISTER2.CHARGER.TYPE;MSIC_REGISTER2.CHARGER.HEALTH;MSIC_REGISTER2.CHARGER.PRESENT</Value>
        </Parameter>
        <Parameter>
            <!-- Charger Type : DCP | SDP (Current=0.5mA) | CDP(Current= 1.5mA) -->
            <Name>CHARGER_TYPE</Name>
            <Value>SDP</Value>
        </Parameter>
        <Parameter>
            <!-- The os to start : can be 'MOS' or 'ROS' -->
            <Name>START_OS</Name>
            <Value>MOS</Value>
        </Parameter>
        <Parameter>
            <!-- Delay in second between starting of test and first measure of ibatt, before charger plug -->
            <Name>DELAY_TO_MEASURE_BEFORE_PLUG</Name>
            <Value>60</Value>
        </Parameter>
        <Parameter>
            <!-- Delay in second between starting of test and second measure of ibatt, after charger plug-->
            <Name>DELAY_TO_MEASURE_AFTER_PLUG</Name>
            <Value>90</Value>
        </Parameter>
    </Parameters>
</TestCase>