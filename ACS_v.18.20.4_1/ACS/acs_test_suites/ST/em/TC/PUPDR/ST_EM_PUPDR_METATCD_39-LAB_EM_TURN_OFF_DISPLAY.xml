<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_TURN_OFF_DISPLAY</UseCase>
    <Description>Monitor that the user can turn off the display.</Description>
    <b2bIteration>1</b2bIteration>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <b2bContinuousMode>False</b2bContinuousMode>
    <Parameters>
        <Parameter>
            <!-- Battery Voltage -->
            <Name>VBATT</Name>
            <Value>3.8</Value>
        </Parameter>
        <Parameter>
            <!-- battery temperature -->
            <Name>BATT_TEMP</Name>
            <Value>25</Value>
        </Parameter>
    </Parameters>
</TestCase>