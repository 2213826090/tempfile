<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_BOOT_PRESS_ON</UseCase>
    <Description>Long keypress on power button while platform is off</Description>
    <b2bIteration>1</b2bIteration>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <b2bContinuousMode>False</b2bContinuousMode>
    <Parameters>
        <Parameter>
            <!-- Battery Voltage (in Volt) -->
            <Name>VBATT</Name>
            <Value>3.8</Value>
        </Parameter>
        <Parameter>
            <!-- Critical Battery Voltage Threshold (in Volt) -->
            <Name>VBATT_CRITICAL</Name>
            <Value>3.3</Value>
        </Parameter>
    </Parameters>
</TestCase>