<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_PS_WATCHDOG_MECHANISM</UseCase>
    <Description>PUPDR Energy Management test watchdog mechanism.</Description>
    <b2bIteration>1</b2bIteration>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <b2bContinuousMode>False</b2bContinuousMode>
    <Parameters>
        <Parameter>
            <!-- Vbatt Voltage -->
            <Name>VBATT</Name>
            <Value>3.8</Value>
        </Parameter>
        <Parameter>
            <!-- type of board freeze to do : SOFT (software) or HARD (hardware) -->
            <Name>FREEZE_TYPE</Name>
            <Value>BOTH</Value>
        </Parameter>
        <Parameter>
            <!-- Maximum time in second to wait before reboot is done since freeze happen -->
            <Name>REBOOT_TIMEOUT</Name>
            <Value>100</Value>
        </Parameter>
    </Parameters>
</TestCase>