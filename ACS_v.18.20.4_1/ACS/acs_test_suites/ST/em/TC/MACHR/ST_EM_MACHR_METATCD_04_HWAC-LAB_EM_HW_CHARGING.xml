<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_HW_CHARGING</UseCase>
    <Description>Platform is OFF and Wall charger is plugged in.</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <Parameters>
        <Parameter>
            <!-- Vbatt Voltage -->
            <Name>VBATT</Name>
            <Value>2.5</Value>
        </Parameter>
        <Parameter>
            <!-- Charger type : DCP or AC_CHGR -->
            <Name>CHARGER_TYPE</Name>
            <Value>AC_CHGR</Value>
        </Parameter>
    </Parameters>
</TestCase>