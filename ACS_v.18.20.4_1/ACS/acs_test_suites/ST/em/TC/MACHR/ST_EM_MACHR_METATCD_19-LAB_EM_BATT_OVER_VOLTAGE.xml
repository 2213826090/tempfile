<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_BATT_OVER_VOLTAGE</UseCase>
    <Description>Monitor battery Charge when setting battery over voltage.</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <Parameters>
        <Parameter>
            <!-- Battery Voltage normal (in Volt) -->
            <Name>VBATT_LOW</Name>
            <Value>3.8</Value>
        </Parameter>
        <Parameter>
            <!-- Battery Voltage over voltage (in Volt) -->
            <Name>VBATT_HIGH</Name>
            <Value>5.2</Value>
        </Parameter>
        <Parameter>
            <!-- Battery Temperature (in degree Celsius) : 95, 70, 50, 25, 10, 5, 0, -15 -->
            <Name>BATTERY_TEMPERATURE</Name>
            <Value>25</Value>
        </Parameter>
        <Parameter>
            <!-- reboot in order to bretray OCV -->
            <Name>REBOOT_FOR_OCV</Name>
            <Value>True</Value>
        </Parameter>
    </Parameters>
</TestCase>