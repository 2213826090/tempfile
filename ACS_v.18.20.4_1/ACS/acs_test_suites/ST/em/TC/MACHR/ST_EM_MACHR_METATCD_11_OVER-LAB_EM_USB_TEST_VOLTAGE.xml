<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_USB_TEST_VOLTAGE</UseCase>
    <Description>Monitor battery Charge when setting USB over or under voltage.</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcMaxAttempt>2</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <Parameters>
        <Parameter>
            <!-- Battery Temperature (in degree Celsius) : 95, 70, 50, 25, 10, 5, 0, -15 -->
            <Name>BATTERY_TEMPERATURE</Name>
            <Value>25</Value>
        </Parameter>
        <Parameter>
            <!-- VUSB normal voltage -->
            <Name>VUSB_NORMAL</Name>
            <Value>6</Value>
        </Parameter>
        <Parameter>
            <!-- VUSB over (8V) or under (4V) voltage -->
            <Name>VUSB_TEST</Name>
            <Value>8</Value>
        </Parameter>
    </Parameters>
</TestCase>