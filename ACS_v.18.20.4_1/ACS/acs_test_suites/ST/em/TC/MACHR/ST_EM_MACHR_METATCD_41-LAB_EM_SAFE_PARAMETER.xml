<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LAB_EM_SAFE_PARAMETER</UseCase>
    <Description>Flash board with a fake IFWI and test that SAFE parameter are enable.</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <Parameters>
        <Parameter>
            <!-- Battery Voltage -->
            <Name>VBATT</Name>
            <Value>3.8</Value>
        </Parameter>
        <Parameter>
            <!-- Battery Full Voltage -->
            <Name>VBATT_FULL</Name>
            <Value>4.2</Value>
        </Parameter>
        <Parameter>
            <!-- Charge Voltage -->
            <Name>VUSB</Name>
            <Value>5.5</Value>
        </Parameter>
        <Parameter>
            <!-- Fake Build Path -->
            <Name>FAKE_BUILD_PATH</Name>
            <Value>C:\_Dev\flash\fake_blankphone.zip;C:\_Dev\flash\fake_buildeng.zip</Value>
        </Parameter>
    </Parameters>
</TestCase>