<?xml version='1.0' encoding='ASCII'?>
<Campaign version="13.49">
    <Parameters>
        <Parameter isControlledPSUsed="False"/>
        <Parameter isIoCardUsed="False"/>
        <!-- Set skipBootOnPowerCycle to True to disable the accs boot procedure -->
        <!-- Important, it means that boot will be handle inside the campaign/tc -->
        <Parameter skipBootOnPowerCycle="False"/>
        <!-- Set bootRetryNumber to maximum number of boot retries during the acs boot procedure -->
        <!-- Important, it means that bootRetryNumber is set by default to 0 if not specified -->
        <Parameter bootRetryNumber="0"/>
        <Parameter runHookScripts="False"/>
        <Parameter powerCycleBetweenTC="False"/>
        <Parameter powerCycleOnFailure="False"/>
        <!-- finalDutState allows user to leave the DUT in a specific state at the end of the campaign
            Possible values: PowerOn | PowerOff | Charging | NoChange
            By default DUT is leaved in NoChange state.
            In case the user selects Charging as final state of the DUT and that DUT fails to enter Charging,
            ACS shall try to switch the DUT off -->
        <Parameter finalDutState="NoChange"/>
        <Parameter stopCampaignOnCriticalFailure="False"/>
        <Parameter stopCampaignOnFirstFailure="False"/>
        <!-- LoggingLevel possible values: info | warning | debug | error -->
        <Parameter loggingLevel="debug"/>
        <!-- CampaignType format -> <TEAM>_<CAMPAIGNTYPE>
            Possible values:
            FT(Feature Team)-> SANITY | REGRESSION | STRESS, RELIABILTY | MTBF
            ST(System Test)-> SANITY | REGRESSION | STRESS, RELIABILTY | MTBF | EM
            SI(System Integration)-> EAT | STAB | PACT | CAT0 | RELIABILITY | MTBF | CTS | ACS_CI
            Otherwise "Others" should be used -->
        <Parameter CampaignType="MTBF"/>
        <Parameter TCRReportViaRESTAPI="True"/>
    </Parameters>
    <Targets>
        <!-- Target Values -->
        <Target targetB2bPassRate="100"/>
    </Targets>
    <TestCases>
        <!-- Before running these tests, place DUT on the NFC robot, also ARTIFACT_MANAGER equipment with URI
         value="https://mcg-depot.intel.com/artifactory/acs_test_artifacts/" must exist in bench config -->

        <!--Setup, installing ACS Agents-->
        <TestCase Id="../../../../../TC/TR/ACS/Communication/Bluetooth/setup/prereq_Bluetooth_SYSTEM_SETUP_EMBEDDED"/>
        <!-- Run iterative NFC TC -->
        <TestCase Id="../../../../../TC/TR/ACS/Communication/NFC/Read_a_Tag_with_web_address_and_connect_to_website_with_WIFI_on-iteratively"/>

    </TestCases>
</Campaign>
