        <TestCase Id="../../../../../TC/TR/ACS/Communication/Bluetooth/STM32F4_Hardware_Implementation/Bluetooth_HID_DUT_pairs_and_connects_BT_Keyboard" />
        <TestCase Id="../../../../../TC/TR/ACS/Communication/Bluetooth/STM32F4_Hardware_Implementation/Bluetooth_HID_HOGT_ONOFF_ITERATIVE_001" />
        <TestCase Id="../../../../../TC/TR/ACS/Communication/Bluetooth/STM32F4_Hardware_Implementation/Bluetooth_HID_Keyboard_interaction_while_scanning" />

