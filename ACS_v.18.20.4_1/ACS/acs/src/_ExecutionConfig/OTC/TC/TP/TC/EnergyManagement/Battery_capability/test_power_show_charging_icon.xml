<?xml version="1.0" ?>
<TestCase>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Discription> 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;Check if Devices charge status icon in top bar when DUT connect/disconnect a charger&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;Pre-condition:(Devices is on, with only battery)&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;Post-condition:(Battery status show &amp;quot;Charging over AC&amp;quot;)&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;Action: DUT connect/disconnect a charger, check top bar battery icon&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;Expected&amp;nbsp; result:&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;plug in charge, the battery icon on top right corner becomes charging mode(flashing lightning appears)&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt; 
&lt;div align=&quot;left&quot; style=&quot;min-height:9pt&quot;&gt;
&lt;font face=&quot;Arial&quot;&gt;&lt;span style=&quot;font-size:8pt&quot;&gt;plug out charge, the battery icon on top right corner becomes discharging mode(flashing lightning disappears)&lt;/span&gt;&lt;/font&gt;
&lt;/div&gt;  
</Discription>
