<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>ANDROID_INST</UseCase>

    <Description/>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>3</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <TestSteps>
        <Initialize>
                <TestStep Id="UNINSTALL_APP"
                          DEVICE="PHONE1"
                          APP_NAME="com.intel.test.systemapitests"
                          TIMEOUT="DEFAULT"
                          BACKUP_APP_NAME="NO_BACKUP"
                          BACKUP_FILE_PATH="APP_BACKUP_PATH"/>
        </Initialize>
        <Setup>
            <TestStep Id="GET_ARTIFACT"
                ARTIFACT="AUDIO/MP3/MP3_24kHz_160kbps_Mono.mp3"
                ARTIFACT_SOURCE="FROM_BENCH:ARTIFACT_MANAGER:URI"
                TRANSFER_TIMEOUT="DEFAULT"
                STORED_FILE_PATH="FILE_PATH_IN_CACHE1"
                EQT="ARTIFACT_MANAGER"/>
            <TestStep Id="INSTALL_FILE" DEVICE="PHONE1"FILE_PATH="FROM_CTX:FILE_PATH_IN_CACHE1"
                TYPE="bin"
                DESTINATION="/storage/sdcard1/" TIMEOUT="DEFAULT"
                DESTINATION_STORED_PATH="FILE_PATH_IN_CACHE1"/>
        </Setup>
        <RunTest/>
        <TearDown/>
        <Finalize>
                <TestStep Id="UNINSTALL_APP"
                          DEVICE="PHONE1"
                          APP_NAME="com.intel.test.systemapitests"
                          TIMEOUT="DEFAULT"
                          BACKUP_APP_NAME="NO_BACKUP"
                          BACKUP_FILE_PATH="APP_BACKUP_PATH"/>
        </Finalize>
    </TestSteps>
    <Parameters>
        <Parameter>
            <Name>TIMEOUT</Name>
            <Value>300</Value>
        </Parameter>
        <Parameter>
            <Name>PROCESS_NAME</Name>
            <Value>com.intel.test.systemapitests/com.intel.test.systemapitests.runners.GenericArgumentPassingTestRunner</Value>
        </Parameter>
        <Parameter>
            <Name>AM_EXTRA</Name>
            <Value>-e class com.intel.test.systemapitests.tests.FileSystemTestsDriver#testCopyAudioAndMediaStoreRefresh -e argsExtra '/mnt/sdcard/MP3_24kHz_128kbps_Stereo.mp3:audio /storage/sdcard1/MP3_24kHz_160kbps_Mono.mp3:audio'</Value>
        </Parameter>
        <Parameter>
            <Name>APKS</Name>
            <Value>INSTRUMENTATION/ANDROID/ApiTestsSystemUID_L_version.apk</Value>
        </Parameter>
        <Parameter>
            <Name>TEST_FILES</Name>
            <Value>AUDIO/MP3/MP3_24kHz_128kbps_Stereo.mp3</Value>
        </Parameter>
        <Parameter>
            <Name>TEST_FILES_DEST</Name>
            <Value>/mnt/sdcard/</Value>
        </Parameter>
        <Parameter>
            <Name>SET_PROPS</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>RETRIEVE_ARTIFACT</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>PRE_REBOOT</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT_NOK</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>TAGS</Name>
            <Value>ABT,UNIT_TESTS,API_TESTS,SYSTEM_STORAGE_USB</Value>
        </Parameter>
    </Parameters>
</TestCase>
