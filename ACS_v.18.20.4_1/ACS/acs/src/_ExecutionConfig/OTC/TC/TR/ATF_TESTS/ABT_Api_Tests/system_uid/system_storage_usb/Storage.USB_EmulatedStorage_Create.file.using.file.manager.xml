<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>ANDROID_INST</UseCase>

    <Description/>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>3</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <TestSteps>
        <Initialize>
                <TestStep Id="UNINSTALL_APP"
                          DEVICE="PHONE1"
                          APP_NAME="com.intel.test.systemapitests"
                          TIMEOUT="DEFAULT"
                          BACKUP_APP_NAME="NO_BACKUP"
                          BACKUP_FILE_PATH="APP_BACKUP_PATH"/>
        </Initialize>
        <Setup/>
        <RunTest/>
        <TearDown/>
        <Finalize>
                <TestStep Id="UNINSTALL_APP"
                          DEVICE="PHONE1"
                          APP_NAME="com.intel.test.systemapitests"
                          TIMEOUT="DEFAULT"
                          BACKUP_APP_NAME="NO_BACKUP"
                          BACKUP_FILE_PATH="APP_BACKUP_PATH"/>
        </Finalize>
    </TestSteps>
    <Parameters>
        <Parameter>
            <Name>TIMEOUT</Name>
            <Value>100</Value>
        </Parameter>
        <Parameter>
            <Name>PROCESS_NAME</Name>
            <Value>com.intel.test.systemapitests/com.intel.test.systemapitests.runners.GenericArgumentPassingTestRunner</Value>
        </Parameter>
        <Parameter>
            <Name>AM_EXTRA</Name>
            <Value>-e class com.intel.test.systemapitests.tests.FileSystemTestsDriver#testCreateFile2 -e args 'createDir:/storage/emulated/0/ fileName:MyTest.txt fileSizeKB:200'</Value>
        </Parameter>
        <Parameter>
            <Name>APKS</Name>
            <Value>INSTRUMENTATION/ANDROID/ApiTestsSystemUID_L_version.apk</Value>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT_NOK</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>TAGS</Name>
            <Value>ABT,UNIT_TESTS,API_TESTS,SYSTEM_STORAGE_USB</Value>
        </Parameter>
    </Parameters>
</TestCase>
