<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
  <ClassName>UseCase.OTC.Communications.Wifi.LAB_CONNECT_TO_AP.ConnectToAP</ClassName>
  <Description>Wifi test case for wpa secured networks</Description>
  <b2bIteration>1</b2bIteration>
  <b2bContinuousMode>False</b2bContinuousMode>
  <Parameters>
    <Parameter>
      <Name>OPERATION_MODE</Name>
      <Value>b</Value>
    </Parameter>
    <Parameter>
      <Name>SECURITY_MODE</Name>
      <Value>wpa_psk</Value>
    </Parameter>
    <Parameter>
      <Name>WIFI_PASSWORD</Name>
      <Value>test1234</Value>
    </Parameter>
 <Parameter>
      <Name>ALGORITHM</Name>
      <Value>aes</Value>
    </Parameter>
    <Parameter>
      <Name>TIMEOUT</Name>
      <Value>60</Value>
    </Parameter>
    <Parameter>
      <Name>TEST_APK</Name>
      <Value>INSTRUMENTATION/ANDROID/ApiTests.apk</Value>
    </Parameter>
    <Parameter>
      <Name>PROCESS_NAME</Name>
      <Value>com.intel.test.apitests/com.intel.test.apitests.runners.WifiEncryptionTestRunner</Value>
    </Parameter>
    <Parameter>
      <Name>AM_EXTRA</Name>
      <Value>-e apName "ddwrt" -e apPassword test1234 -e apTestPage http://192.168.1.1/ -e class com.intel.test.apitests.tests.WiFiConnectionTest#testWifiAP_Wpa</Value>
    </Parameter>
  </Parameters>
</TestCase>
