<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>Testlib</UseCase>
    <Description>WATCHDOG_COUNTER_MAX shall be configurable with adb command</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>1</TcMaxAttempt>
    <Parameters/>
    <Domain>System_FastBoot</Domain>
    <Parameters/>
    <Phase>CORE</Phase>
    <Parameters/>
    <Type>FUNCTIONAL</Type>
    <Parameters>
        <Parameter>
            <Name>timeout</Name>
            <Value>1200</Value>
        </Parameter>
        <Parameter>
            <Name>test</Name>
            <Value>scripts/android/fastboot/tests/panic_to_crashmode.py</Value>
        </Parameter>
        <Parameter>
            <Name>script-args</Name>
            <Value>watchdog_counter_max=0 create_crash_mode=watchdog use_delay=False use_combo_button=False</Value>
        </Parameter>
    </Parameters>
</TestCase>