<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>Testlib</UseCase>
    <Description>Test Brillo wifi AP mode</Description>
    <Description>Command:adb shell ifconfig</Description>
    <Description>Command:adb shell iw dev wlan0 info</Description>
    <Description>Result: wlan0 interface should be up</Description>
    <Description>Result: device should be in AP mode</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>1</TcMaxAttempt>
    <Parameters>
        <Parameter>
            <Name>test</Name>
            <Value>scripts/brillo/tests/7.0.0_wifi_ap_mode.py</Value>
        </Parameter>
        <Parameter>
            <Name>script-args</Name>
            <Value>default_ip=192.168.0.254</Value>
        </Parameter>
    </Parameters>
</TestCase>
