        <Parameter>
            <Name>RETRIEVE_ARTIFACT</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>PRE_REBOOT</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT_NOK</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>TAGS</Name>
            <Value>ABT,UNIT_TESTS,API_TESTS,FILESYSTEM</Value>
        </Parameter>
    </Parameters>
