        <Finalize>
        </Finalize>
    </TestSteps>
    <Parameters>
        <Parameter>
            <Name>TIMEOUT</Name>
            <Value>500</Value>
        </Parameter>
        <Parameter>
            <Name>PROCESS_NAME</Name>
            <Value>com.intel.test.apitests/com.intel.test.apitests.runners.CameraConfigTestRunner</Value>
        </Parameter>
        <Parameter>
            <Name>AM_EXTRA</Name>
            <Value>-e class com.intel.test.apitests.tests.CameraTestsDriver#testTakePictureWhilePlayingMusic -e cameraID 0 -e nrOfTests 10 -e width 1280 -e height 720 -e testType picture -e extraParams 'musicFile:multithread_audio.mp3'</Value>
