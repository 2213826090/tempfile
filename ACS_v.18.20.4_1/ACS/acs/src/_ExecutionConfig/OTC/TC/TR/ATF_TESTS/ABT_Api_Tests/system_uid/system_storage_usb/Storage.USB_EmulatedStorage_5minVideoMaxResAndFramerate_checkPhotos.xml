<?xml version='1.0' encoding='ASCII'?>
<TestCase>
    <UseCase>ANDROID_INST</UseCase>

    <Description/>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>3</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <TestSteps>
        <Initialize>
            <TestStep Id="GET_ARTIFACT" EQT="ARTIFACT_MANAGER" ARTIFACT="FROM_BENCH:ARTIFACT_APKS:ApiTests" ARTIFACT_SOURCE="FROM_BENCH:ARTIFACT_MANAGER:URI" TRANSFER_TIMEOUT="DEFAULT" STORED_FILE_PATH="FILE_PATH_IN_CACHE1"/>
            <TestStep Id="INSTALL_APP"
                      DEVICE="PHONE1"
                      SIGN="False"
                      SIGN_KEY_PATH="DEFAULT"
                      FILE_PATH="FROM_CTX:FILE_PATH_IN_CACHE1"
                      BACKUP="False"
                      TIMEOUT="30"
                      BACKUP_FILE_PATH="/backup"/>
        </Initialize>
    </TestSteps>
    <Parameters>
        <Parameter>
            <Name>TIMEOUT</Name>
            <Value>1000</Value>
        </Parameter>
        <Parameter>
            <Name>PROCESS_NAME</Name>
            <Value>com.intel.test.apitests/com.intel.test.apitests.runners.CameraConfigTestRunner</Value>
        </Parameter>
        <Parameter>
            <Name>AM_EXTRA</Name>
            <Value>-e class com.intel.test.apitests.tests.CameraTestsDriver#testCaptureMediaAndCheckMediaStore -e cameraID 0 -e recordingTime 300000 -e nrOfTests 1 -e width 1280 -e height 720 -e testType video</Value>
        </Parameter>
        <Parameter>
            <Name>TEST_FILES</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>TEST_FILES_DEST</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>SET_PROPS</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>RETRIEVE_ARTIFACT</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>PRE_REBOOT</Name>
            <Value/>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>POST_REBOOT_NOK</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <Name>TAGS</Name>
            <Value>ABT,UNIT_TESTS,API_TESTS,SYSTEM_STORAGE_USB</Value>
        </Parameter>
    </Parameters>
</TestCase>
