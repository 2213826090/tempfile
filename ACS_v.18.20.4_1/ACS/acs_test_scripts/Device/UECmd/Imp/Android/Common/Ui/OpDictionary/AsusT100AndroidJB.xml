<?xml version="1.0" encoding="UTF-8"?>
<operationsets>
    <operationset name="phone_unlock">
        <operation>
            <command comment="wake the screen of the device">wake</command>
            <parameter comment="seconds to wait before next action">2</parameter>
        </operation>
        <operation>
            <command comment="Unlock the phone">drag</command>
            <parameter comment="x coordinate of the starting point">890</parameter>
            <parameter comment="y coordinate of the starting point">370</parameter>
            <parameter comment="x coordinate of the ending point">1140</parameter>
            <parameter comment="y coordinate of the ending point">370</parameter>
            <parameter comment="seconds to wait before next action">1</parameter>
        </operation>
    </operationset>
    <operationset name="go_home">
        <operation>
            <command comment="press the enter key to finish the input">keypress</command>
            <parameter comment="the key to be pressed">KEYCODE_HOME</parameter>
            <parameter comment="seconds to wait before next action">2</parameter>
            <logcat_ouput></logcat_ouput>
        </operation>
    </operationset>
