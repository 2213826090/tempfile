<ModuleConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="FlashModule.xsd">

    <!-- FLASH MODULE PARAMETERS -->

    <!-- MODEM_FLASH: remove modem flash command in fastboot flash.xml -->
    <MODEM_FLASH>true</MODEM_FLASH>
    <!-- DEDIPROG: set to true if board possesses dediprog ability -->
    <DEDIPROG>true</DEDIPROG>
    <!-- RECOVERY: set to true to force use of dediprog to flash bios -->
    <RECOVERY>false</RECOVERY>

    <!-- PUPDR_COMMON PARAMETERS -->

    <!-- PARTITIONS: list here the partitions that need to be checks on MOS boot -->
    <!-- separate partitions with ";" -->
    <PARTITIONS>/factory;/config;/cache;/system</PARTITIONS>
    <!-- MODEM: set to true if board does not possess any modem-->
    <MODEM>false</MODEM>
    <!-- SHELL_IN_ROS: set to false if board has no shell in ROS -->
    <SHELL_IN_ROS>false</SHELL_IN_ROS>
    <!-- CHECK_FASTBOOT: (un)able fastboot image check after flash -->
    <CHECK_FASTBOOT>false</CHECK_FASTBOOT>
    <!-- CHECK_RECOVERY: (un)able recovery image check after flash -->
    <CHECK_RECOVERY>false</CHECK_RECOVERY>
    <!-- CHECK_OSLOADER: (un)able osloader efi application check after flash -->
    <CHECK_OSLOADER>false</CHECK_OSLOADER>
    <!-- CRASHLOGS: set to false to prevent history_event checks -->
    <CRASHLOGS>false</CRASHLOGS>
    <!-- PKP_5S: set if 5S power_key press does not shut down the board -->
    <PKP_5S>false</PKP_5S>

</ModuleConfiguration>
