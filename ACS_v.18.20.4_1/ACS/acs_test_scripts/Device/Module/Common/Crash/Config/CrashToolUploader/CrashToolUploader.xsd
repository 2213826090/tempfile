<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="ModuleConfiguration">
        <xs:complexType>
         <xs:sequence>
            <xs:element name="disableCLOTA" type="xs:boolean"/>
            <xs:element name="uploadCrashToServer" type="xs:boolean"/>
            <xs:element name="fetchCrashToHost" type="xs:boolean"/>
            <xs:element name="serverAddress" type="xs:string"/>
            <xs:element name="serverPort" type="xs:int"/>
            <xs:element name="debugMode" type="xs:boolean"/>
            <xs:element name="nbCrashLogProcessingLimit" type="xs:int"/>
        </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
