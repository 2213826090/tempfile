<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LIVE_CAMERA_FLASH</UseCase>
    <Description>Use Camera to captue image</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- Flash mode to set, possible values :
              - auto
              - off
              - on
              - torch
              - red-eye
               -->
            <Name>FLASH_MODE</Name>
            <Value>on</Value>
        </Parameter>
        <Parameter>
            <!-- Directory where to save picture taken, can be any existing directory in the DUT  -->
            <Name>SAVE_DIRECTORY</Name>
            <Value>sdcard/</Value>
        </Parameter>
    </Parameters>
</TestCase>
