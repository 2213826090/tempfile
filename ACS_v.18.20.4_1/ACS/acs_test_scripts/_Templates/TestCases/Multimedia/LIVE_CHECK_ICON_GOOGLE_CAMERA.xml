<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LIVE_CHECK_ICON_GOOGLE_CAMERA</UseCase>
    <Description>Check if icons are correctly displayed in google camera</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>3</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <Parameters>
        <Parameter>
            <!--List of icon in camera mode-->
            <Name>ICON_DICT_MODE</Name>
            <Value>id:id,id:id</Value>
        </Parameter>
        <Parameter>
            <!--List of icon in camera option-->
            <Name>ICON_DICT_OPTION</Name>
            <Value>id:id,id:id</Value>
        </Parameter>
        <Parameter>
            <!--Type of Hardware (tablet or phone)-->
            <Name>HARDWARE</Name>
            <Value>phone</Value>
        </Parameter>
    </Parameters>
</TestCase>
