<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase>
    <UseCase>LIVE_CHECK_ICON_GOOGLE_CAMERA_ROTATION</UseCase>
    <Description>Check if icons are correctly displayed in google camera with rotation</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcMaxAttempt>3</TcMaxAttempt>
    <TcAcceptanceCriteria>1</TcAcceptanceCriteria>
    <Parameters>
        <Parameter>
            <!--List of icon in camera mode text-->
            <Name>ICON_DICT_TEXT</Name>
            <Value>text:text,text:text</Value>
        </Parameter>
        <Parameter>
            <!--List of icon in camera mode id-->
            <Name>ICON_DICT_ID</Name>
            <Value>id:id,id:id</Value>
        </Parameter>
        <Parameter>
            <!--Rotation during test (landscape for phone and portrait for tablet)-->
            <Name>ROTATION</Name>
            <Value>landscape</Value>
        </Parameter>
    </Parameters>
</TestCase>
