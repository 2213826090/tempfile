<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LAB_WEB_BROWSING</UseCase>
    <Description>LAB Web browsing Test Case</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- Cell Technology (2G|3G) -->
            <Name>CELL_TECH</Name>
            <Value>2G</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Band
                    For 2G : EGSM|GSM850|PCS|DCS
                    For 3G : 1,2,5,8
            -->
            <Name>CELL_BAND</Name>
            <Value>EGSM</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Service ( (GSM|GPRS|EGPRS) for 2G CELL, (CIRCUIT|PACKET|CIRCUIT_PACKET) for 3G CELL) -->
            <Name>CELL_SERVICE</Name>
            <Value>EGPRS</Value>
        </Parameter>
        <Parameter>
            <!-- ARFCN -->
            <Name>ARFCN</Name>
            <Value>20</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Power -->
            <Name>CELL_POWER</Name>
            <Value>-75</Value>
        </Parameter>
        <Parameter>
            <!-- Cell LAC, For 2G and 3G: 0 to 65535-->
            <Name>LAC</Name>
            <Value>10</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Routing Area, For 2G and 3G: 1 to 255-->
            <Name>RAC</Name>
            <Value>25</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Country Code 0 to 999 -->
            <Name>MCC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Network Code 0 to 999 -->
            <Name>MNC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Browser to use.Possible values : "NATIVE", "CHROME", "ACS_AGENT"
                Warning ! NATIVE mode is not verbose in term of verdict, because it uses the default browser of the system.
                So the user should check manually in this mode if the page is correctly loaded -->
            <Name>BROWSER_TYPE</Name>
            <Value>ACS_AGENT</Value>
        </Parameter>
        <Parameter>
            <!-- Website url to open String as follow : http://www.intel.com
            For Multiple page loading separate each address by a ";". -->
            <Name>WEBSITE_URL</Name>
            <Value>http://www.intel.com</Value>
        </Parameter>
        <Parameter>
            <!-- Timeout (in second) to load web page -->
            <Name>TIMEOUT</Name>
            <Value>10</Value>
        </Parameter>
    </Parameters>
</TestCase>