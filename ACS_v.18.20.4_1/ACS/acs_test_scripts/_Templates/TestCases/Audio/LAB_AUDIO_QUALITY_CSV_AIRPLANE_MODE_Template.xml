<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LAB_AUDIO_QUALITY_CSV_AIRPLANE_MODE</UseCase>
    <Description>Check Audio Quality during GSM Mobile Originated Call on real network with a specified accessories after switch to airplane mode</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <Parameters>
        <Parameter>
            <!-- Call origin (MO|MT) -->
            <Name>CALL_ORIGIN_TYPE</Name>
            <Value>MO</Value>
        </Parameter>
        <Parameter>
            <!-- Accessorie type (EARPIECE|SPEAKER|HEADSET|BLUETOOTHA2DP|BLUETOOTHHSP) -->
            <Name>ACC_TYPE</Name>
            <Value>SPEAKER</Value>
        </Parameter>
        <Parameter>
             <!-- >Stream Call Volume in percent of dut phone -->
            <Name>CALL_VOLUME_DUT</Name>
            <Value>70</Value>
        </Parameter>
        <Parameter>
             <!-- >Stream Call Volume in percent of ref phone -->
            <Name>CALL_VOLUME_REF</Name>
            <Value>70</Value>
        </Parameter>
        <Parameter>
            <!-- Call end type (NR|MR) -->
            <Name>CALL_END_TYPE</Name>
            <Value>MR</Value>
        </Parameter>
        <Parameter>
            <!-- Call type (2G|3G) -->
            <Name>CALL_TYPE</Name>
            <Value>3G</Value>
        </Parameter>
        <Parameter>
            <!-- Signal tested direction (UL|DL) -->
            <Name>SIGNAL_TESTED_DIRECTION</Name>
            <Description>UL</Description>
        </Parameter>
        <Parameter>
            <!-- Call duration in seconds (default value: 5 secs) -->
            <Name>DURATION</Name>
            <Value>20</Value>
        </Parameter>
    </Parameters>
</TestCase>