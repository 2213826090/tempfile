<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LIVE_SYSTEM_RETRIEVE_SYSTEM_INFO</UseCase>
    <Description>Retrieve system information</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!--The type of information to be retrieved
                The possible value could be:
                /BuildID/Product/CPUABI/Manufacture/Brand/Model
                /User/Host/Hardware/ReleaseVersion/SDKVersion/DisplayWidth
                /DisplayHeight/DisplayPixelFormat/RefreshRate/DisplayDensity
                /DisplayXDPI/DisplayYDPI
            -->
            <Name>TYPE</Name>
            <Value>id</Value>
        </Parameter>
        <Parameter>
            <!--The value of expected, this will different according to which type of
                information to be retrieved, and also can leave it as "reserve", in this condition
                use case will just check the information exists or not.
            -->
            <Name>VALUE</Name>
            <Value>"reserve"</Value>
        </Parameter>
    </Parameters>
</TestCase>