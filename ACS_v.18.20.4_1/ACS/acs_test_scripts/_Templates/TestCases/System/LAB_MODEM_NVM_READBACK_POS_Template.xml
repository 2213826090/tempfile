<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LAB_MODEM_NVM_READBACK_POS</UseCase>
    <Description>Check the modem's NVM configuration when DUT is booted in Provisioning OS mode (POS).</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- A coma separated list of prefixes.
                All lines starting with one of those prefixes will
                be taken into account when checking the modem
                configuration.
            -->
            <Name>PREFIXES</Name>
            <Value>nvm;gticom</Value>
        </Parameter>
        <Parameter>
            <!-- The path to the NVM configuration file.
                This text file contains lines with <key>=<value>
            -->
            <Name>NVM_CONFIGURATION_FILE_PATH</Name>
            <Value>./_ExecutionConfig/ModemFlashFiles/nvm.txt</Value>
        </Parameter>
    </Parameters>
</TestCase>