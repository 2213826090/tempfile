<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LAB_FIT_EGPRS_VC_WEB_BROWSING</UseCase>
    <Description>2G - VC + web browsing</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>False</b2bContinuousMode>
    <TcAcceptanceCriteria>PASS</TcAcceptanceCriteria>
    <TcMaxAttempt>1</TcMaxAttempt>
    <Parameters>
        <Parameter>
            <!-- Cell Band (PGSM|RGSM|EGSM|GSM450|GSM480|GSM750|GSM850|TGSM810|PCS|DCS) -->
            <Name>CELL_BAND</Name>
            <Value>DCS</Value>
        </Parameter>
        <Parameter>
            <!-- Broadcast Channel Arfcn frequency -->
            <Name>BCH_ARFCN</Name>
            <Value>512</Value>
        </Parameter>
        <Parameter>
            <!-- Traffic Channel Arfcn frequency -->
            <Name>TCH_ARFCN</Name>
            <Value>698</Value>
        </Parameter>
        <Parameter>
            <!-- Packet Data Traffic Channel Arfcn frequency -->
            <Name>PDTCH_ARFCN</Name>
            <Value>512</Value>
        </Parameter>
        <Parameter>
            <!-- Cell power in dBm -->
            <Name>CELL_POWER</Name>
            <Value>-60</Value>
        </Parameter>
        <Parameter>
            <!-- Multislot configuration
                D1U1, D1U2, D1U3, D1U4, D1U5, D1U6, D2U1, D2U2, D2U3, D2U4, D2U5,
                D3U1, D3U2, D3U3, D3U4, D4U1, D4U2, D4U3, D5U1, D5U2, D6U1 -->
            <Name>MULTISLOT_CONFIG</Name>
            <Value>D3U2</Value>
        </Parameter>
        <Parameter>
            <!-- Uplink Modulation Coding scheme (MCS1|MCS2|MCS3|MCS4|MCS5|MCS6|MCS7|MCS8|MCS9) -->
            <Name>UL_MCS</Name>
            <Value>MCS9</Value>
        </Parameter>
        <Parameter>
            <!-- Downlink Modulation Coding scheme (MCS1|MCS2|MCS3|MCS4|MCS5|MCS6|MCS7|MCS8|MCS9) -->
            <Name>DL_MCS</Name>
            <Value>MCS9</Value>
        </Parameter>
        <Parameter>
            <!-- Puncturing Modulation Coding scheme
                PS1 | PS2 | PS3 for DL_MCS 3,4,7,8,and 9
                PS1 | PS2 for DL_MCS 1, 2, 5, and 6 -->
            <Name>PS_MCS</Name>
            <Value>PS1</Value>
        </Parameter>
        <Parameter>
            <Name>CALL_SETUP_TIMEOUT</Name>
            <Value>60</Value>
        </Parameter>
        <Parameter>
            <!-- Transfer timeout in s -->
            <Name>TIMEOUT</Name>
            <Value>120</Value>
        </Parameter>
        <Parameter>
            <!-- Dialled Phone number (MSISDN). If value is [PHONE_NUMBER] the value used will be the phoneNumber defined in the Phone_Catalog.xml -->
            <Name>PHONE_NUMBER</Name>
            <Value>123</Value>
        </Parameter>
        <Parameter>
            <!-- Browser to use.Possible values : "NATIVE", "ACS_AGENT"
                Warning ! NATIVE mode is not verbose in term of verdict, because it uses the default browser of the system.
                So the user should check manually in this mode if the page is correctly loaded -->
            <Name>BROWSER_TYPE</Name>
            <Value>ACS_AGENT</Value>
        </Parameter>
        <Parameter>
            <!-- Website url to open String as follow : http://www.intel.com -->
            <Name>WEBSITE_URL</Name>
            <Value></Value>
        </Parameter>
    </Parameters>
</TestCase>
