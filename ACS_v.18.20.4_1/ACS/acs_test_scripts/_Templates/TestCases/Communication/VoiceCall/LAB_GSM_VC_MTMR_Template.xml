<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LAB_GSM_VC_MTMR</UseCase>
    <Description>GSM Mobile Terminated Call / Mobile Release on simulated network</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- Cell Band (PGSM|RGSM|EGSM|GSM450|GSM480|GSM750|GSM850|TGSM810|PCS|DCS) -->
            <Name>CELL_BAND</Name>
            <Value>DCS</Value>
        </Parameter>
        <Parameter>
            <!-- Broadcast Channel Arfcn frequency -->
            <Name>BCH_ARFCN</Name>
            <Value>698</Value>
        </Parameter>
        <Parameter>
            <!-- Traffic Channel Arfcn frequency -->
            <Name>TCH_ARFCN</Name>
            <Value>650</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Service (GSM|GPRS|EGPRS) -->
            <Name>CELL_SERVICE</Name>
            <Value>GSM</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Power (in dBm) -->
            <Name>CELL_POWER</Name>
            <Value>-60</Value>
        </Parameter>
