<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.37">
    <UseCase>LAB_LTE_SMS_SGS</UseCase>
    <Description>SMS SGS MO/MT over LTE simulated network</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- Multiple-Input Multiple-Output to set : true or false -->
            <Name>MIMO</Name>
            <Value>false</Value>
        </Parameter>
        <Parameter>
            <!-- Operation mode to set : BSE or SA -->
            <Name>SIGNAL_MODE</Name>
            <Value>BSE</Value>
        </Parameter>
        <Parameter>
            <!-- Physical Cell ID : 0 to 503 -->
            <Name>PHYSICAL_CELL_ID</Name>
            <Value>0</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Setup : "A" for A-cell or "B" for B-cell -->
            <Name>CELL_ID</Name>
            <Value>A</Value>
        </Parameter>
        <Parameter>
            <!-- Cell power for RFO1 -->
            <Name>CELL_POWER_RFO1</Name>
            <Value>-40</Value>
        </Parameter>
        <Parameter>
            <!-- Cell power for RFO2 -->
            <Name>CELL_POWER_RFO2</Name>
            <Value>-40</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Country Code 0 to 999 -->
            <Name>MCC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Network Code 0 to 999 -->
            <Name>MNC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Band (from Band 1 to Band 32) -->
            <Name>CELL_BAND</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Downlink channel -->
            <Name>DL_EARFCN</Name>
            <Value>300</Value>
        </Parameter>
        <Parameter>
            <!-- Scenario filename and path to load on PXT -->
            <Name>SCENARIO_PATH</Name>
            <Value></Value>
        </Parameter>
        <Parameter>
            <!-- INCOMING_NUMBER-->
            <Name>INCOMING_NUMBER</Name>
            <Value></Value>
        </Parameter>
        <Parameter>
            <!-- SMS DIRECTION: (MO [Mobile Originated]|MT [Mobile Terminated])-->
            <Name>SMS_DIRECTION</Name>
            <Value>MT</Value>
        </Parameter>
        <Parameter>
            <!-- SMS TEXT Max Length 160 characters-->
            <Name>SMS_TEXT</Name>
            <Value>LTE SMS SG MT Test</Value>
        </Parameter>
        <Parameter>
            <!--SMS transfer TIMEOUT -->
            <Name>SMS_TRANSFER_TIMEOUT</Name>
            <Value>10</Value>
        </Parameter>
    </Parameters>
</TestCase>
