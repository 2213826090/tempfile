<?xml version="1.0" encoding="iso-8859-1"?>
<TestCase version="13.45">
    <UseCase>LAB_WCDMA_SMS_CS_MT</UseCase>
    <Description>WCDMA Mobile Terminated SMS over CS on simulated network</Description>
    <b2bIteration>1</b2bIteration>
    <b2bContinuousMode>True</b2bContinuousMode>
    <TcExpectedResult>PASS</TcExpectedResult>
    <Parameters>
        <Parameter>
            <!-- Home Public Land Mobile Network Coverage (True | False)-->
            <Name>HPLMN_COVERAGE</Name>
            <Value>False</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Country Code 0 to 999 as string. If MCC is empty, no operator check is done-->
            <Name>MCC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Mobile Network Code 0 to 99 as string. If MNC is empty, no operator check is done -->
            <Name>MNC</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Cell Band (from Band 1 to Band 8) -->
            <Name>CELL_BAND</Name>
            <Value>1</Value>
        </Parameter>
        <Parameter>
            <!-- Cell power in dB -->
            <Name>CELL_POWER</Name>
            <Value>-60</Value>
        </Parameter>
        <Parameter>
            <!-- Downlink UARFCN Frequency -->
            <Name>DL_UARFCN</Name>
            <Value>10700</Value>
        </Parameter>
        <Parameter>
            <!-- SMS text to send -->
            <Name>SMS_TEXT</Name>
            <Value>This is my SMS text !</Value>
        </Parameter>
        <Parameter>
            <!-- SMS Data coding scheme in hexadecimal -->
            <Name>DATA_CODING_SCHEME</Name>
            <Value>00</Value>
        </Parameter>
        <Parameter>
            <!-- SMS transfer timeout in seconds -->
            <Name>SMS_TRANSFER_TIMEOUT</Name>
            <Value>10</Value>
        </Parameter>
    </Parameters>
</TestCase>
