    <UseCase Id="LIVE_LAB_NFC_DEBUG_TEST" Domain="CWS" SubDomain="NFC" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.LocalConnectivity.LIVE_LAB_NFC_DEBUG_TEST.LiveLabNfcDebugTest</ClassName>
        <Description>Run whole available NFC UECmd functions</Description>
        <Environment name="CWS_WIFI">
            <Dependency>EQT_NFC_MICROBOT</Dependency>
            <Dependency>EQT_NFC_READER</Dependency>
            <Dependency>EQT_NFC_EMULATOR</Dependency>
        </Environment>
        <Parameters>
             <Parameter name="USE_ROBOT" type="BOOLEAN" isOptional="false">
                <!-- Run the tests that use a Robot -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue>TRUE</DefaultValue>
                <Blank/>
             </Parameter>
             <Parameter name="USE_SECURE_ELEMENT" type="BOOLEAN" isOptional="false">
                <!-- Run the tests that use a Secure element (sim card needed) -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue>TRUE</DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="SECURE_ELEMENT" type="STRING" isOptional="true">
                <!-- Secure element to test -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue>TRUE</DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="NFC_BEAM_INITIAL_STATE" type="BOOLEAN" isOptional="false">
                <!-- If beam activated -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue>TRUE</DefaultValue>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
    <UseCase Id="LAB_NFC_CARD_EMUL_DETECTION" Domain="CWS" SubDomain="NFC" OS="ANDROID" Status="DONE">
