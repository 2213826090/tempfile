        <UseCase Id="LAB_NFC_DUAL_PHONE_BEAM_TRANSFERT" Domain="CWS" SubDomain="NFC" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.LocalConnectivity.LAB_NFC_DUAL_PHONE_BEAM_TRANSFERT.LabNfcDualPhoneBeamTransfert</ClassName>
        <Description>Beam reliability test</Description>
        <Environment name="CWS_NFC">
            <Dependency>EQT_NFC_MICROBOT</Dependency>
        </Environment>
        <Parameters>
             <Parameter name="NFC_BEAM_INITIAL_STATE" type="BOOLEAN" isOptional="false">
                <!-- If beam activated -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue>TRUE</DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="FILE_TO_SHARE" type="STRING" isOptional="false">
                <!-- Name of the file to be shared -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue></DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="BEAM_DURATION" type="DURATION_IN_SEC" isOptional="false">
                <!-- Time to wait for beam -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue></DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="BEAM_PATH" type="STRING" isOptional="false">
                <!-- Path beamed files are stored -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue></DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="DIRECTION" type="STRING" isOptional="false">
                <!-- SEND  or RECEIVE or FULL -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue></DefaultValue>
                <Blank/>
            </Parameter>
            <Parameter name="REF_PHONE_POSITION" type="STRING" isOptional="false">
                <!-- position of ref phone Center or SecondPhone-->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue></DefaultValue>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
    <UseCase Id="LAB_NFC_FIELD_MAPPING" Domain="CWS" SubDomain="NFC" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.LocalConnectivity.LAB_NFC_FIELD_MAPPING.LabNfcFieldMapping</ClassName>
        <Description>Build the nfc field mapping</Description>
        <Environment name="CWS_NFC">
            <Dependency>EQT_NFC_MICROBOT</Dependency>
        </Environment>
        <Parameters>
            <Parameter name="STARTING_Z" type="STRING" isOptional="false">
                <Description>Height where the test is starting 0 by default </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MAX_Z" type="STRING" isOptional="false">
                <Description>Height where the test is ending 0 by default -90000 </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="RIGHT_UP_CORNER" type="STRING" isOptional="false">
                <Description>position to start the test in robot step (10000 = 1 cm)</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="RESULT_FILE_NAME" type="STRING" isOptional="false">
                <Description>file name for the results</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="RESOLUTION" type="STRING" isOptional="false">
                <Description>resolution in robot step of measurement</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="FOUND_MAX_Z" type="STRING" isOptional="false">
                <Description>is test is perform higher z find</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
