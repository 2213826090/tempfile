    <UseCase Id="LIVE_SYSTEM_CHECK_LAUNCHER" Domain="SYSTEM" SubDomain="MISC" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.System.LIVE_SYSTEM_CHECK_LAUNCHER.LiveSystemCheckLauncher</ClassName>
        <Description>
            Checks if the given launcher application package is installed on device and available for user selection.
            It will search the given package on all installed packages, enabling it if necessary (only during the execution on the UC).
            It is also possible to make a chooser window visible, allowing user to select launcher to use for semi-auto testing purpose.
        </Description>
        <Environment name="ANY">
        </Environment>
        <Parameters>
            <Parameter name="LAUNCHER_NAME" type="STRING" isOptional="false">
                <Description>Launcher application package name to search. (eg. com.android.launcher for default android launcher)</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="OPEN_CHOOSER" type="BOOLEAN" isOptional="false">
                <Description>Determines if chooser window for launchers is visible. (true=visible)</Description>
                <PossibleValues/>
                <DefaultValue>false</DefaultValue>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
