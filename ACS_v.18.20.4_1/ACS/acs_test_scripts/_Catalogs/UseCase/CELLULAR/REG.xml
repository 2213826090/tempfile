    <UseCase Id="LAB_LTE_IMS_REG" Domain="CELLULAR" SubDomain="REG" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.Networking.LAB_LTE_IMS_REG.LabLteImsReg</ClassName>
        <Description>Registration over LTE simulated network and then Ping</Description>
        <Environment name="TEL">
            <Capabilities>EQT_RS_CMW500</Capabilities>
        </Environment>
        <Parameters>
            <Parameter name="IMS" type="IS_IMS_ENABLED" isOptional="false">
                <!-- Description: IMS service status on call box : on or off -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MIMO" type="IS_MIMO_ENABLED" isOptional="false">
                <!-- Description: Multiple-Input Multiple-Output to set : TRUE or FALSE -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="ANTENNAS_NUMBER" type="ANTENNA" isOptional="false">
                <!-- Description: number of antennas to set : 1 or 2 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="TRANSMISSION_MODE" type="LTE_TRANSMISSION_MODE" isOptional="false">
                <!-- Description: TRANSMISSION_MODE to set: TM1, TM2, TM3,TM4 ... -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DL_RB_SIZE" type="LTE_RB_SIZE" isOptional="false">
                <!-- Description: DL RB_SIZE to set: 1.4 MHz = 1 to 6 3 MHz = 1 to 15 5 MHz = 1 to 25 10 MHz = 1 to 50 15 MHz = 1 to 75 20 MHz = 1 to 100 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="UL_RB_SIZE" type="LTE_RB_SIZE" isOptional="false">
                <!-- Description: UL RB_SIZE to set: 1.4 MHz = 1 to 6 3 MHz = 1 to 15 5 MHz = 1 to 25 10 MHz = 1 to 50 15 MHz = 1 to 75 20 MHz = 1 to 100 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="TYPE0_BITMAP" type="TYPE0_BITMAP" isOptional="true">
                <!-- Description: TYPE0_BITMAP to set: requires to set TM3 or TM4 profile, MIMO to be set in scenario file 1.4 MHz = 1 to 63 (0x3F) 3 MHz = 1 to 255 (0xFF) 5 MHz = 1 to 8191 (0x1FFF) 10 MHz = 1 to 131071 (0x1FFFF) 15 MHz = 1 to 524287 (0x7FFFF) 20 MHz = 1 to 33554431 (0x1FFFFFF) -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="UL_GRANT_MODE" type="UL_GRANT_MODE" isOptional="false">
                <!-- Description: UL GRANT MODE to set: AUTO or FIXEDMAC -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DL_I_MCS" type="DL_I_MCS" isOptional="false">
                <!-- Description: DL_I_MCS to set: 0 to 28 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="UL_I_MCS" type="UL_I_MCS" isOptional="false">
                <!-- Description: UL_I_MCS to set: 0 to 23, and 29 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_CHANNEL_BANDWIDTH" type="LTE_CELL_CHANNEL_BANDWIDTH" isOptional="false">
                <!-- Description: Cell Channel Bandwidth in MHz -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="SIGNAL_MODE" type="LTE_SIGNAL_MODE" isOptional="false">
                <!-- Description: Operation mode to set : BSE or SA -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="PHYSICAL_CELL_ID" type="LTE_PHYSICAL_CELL_ID" isOptional="false">
                <!-- Description: Physical Cell ID : 0 to 503 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_ID" type="CELL_ID" isOptional="true">
                <!-- Description: Cell Setup : "A" for A-cell or "B" for B-cell -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_POWER_RFO1" type="CELL_POWER_IN_DBM" isOptional="false">
                <!-- Description: Cell power for RFO1 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_POWER_RFO2" type="CELL_POWER_IN_DBM" isOptional="false">
                <!-- Description: Cell power for RFO2 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MCC" type="MCC" isOptional="false">
                <!-- Description: Mobile Country Code 0 to 999 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MNC" type="MNC" isOptional="false">
                <!-- Description: Mobile Network Code 0 to 999 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_BAND" type="CELL_BAND_4G" isOptional="false">
                <!-- Description: Cell Band (from Band 1 to Band 32 for FDD; from Band 33 to Band 63 for TDD) -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DL_EARFCN" type="DL_EARFCN" isOptional="false">
                <!-- Description: Downlink channel -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="IP_VERSION" type="IP_VERSION" isOptional="true">
                <!-- Description>the IP version to use to connect to the mobile network. IPV4 | IPV6 | IPV4V6 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="SCENARIO_PATH" type="SCENARIO_PATH" isOptional="true">
                <!-- Description: Path to the scenario filename to load on PXT -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
</UseCases>
