    <UseCase Id="LIVE_LTE_IMS_REG" Domain="CELLULAR" SubDomain="REG" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.Networking.LIVE_LTE_IMS_REG.LiveLteImsReg</ClassName>
        <Description>IMS registration over LTE</Description>
        <Environment name="TEL">
            <Capabilities>EQT_LIVE_USIM</Capabilities>
        </Environment>
        <Parameters>
            <Parameter name="IMS_REGISTRATION_TIMEOUT" type="TIMEOUT_IN_SEC" isOptional="false">
                <Description>The additional timeout for IMS registration.</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="IMS_PARAMETER_FILE" type="IMS_CONFIG_FILE" isOptional="false">
                <Description> The path to the IMS parameter file to use to create the IMS_Config.ini file on the device. </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="IMS_AUTHENTICATION" type="IMS_PARAMETER" isOptional="false">
                <Description>The authentication mode to use ("digest"|"aka").</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="IMS_PRECONDITIONS" type="IMS_PARAMETER" isOptional="false">
                <Description>The precondition parameters ("3gpp" with preconditions, "ietf" without preconditions).</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="IMS_DIGEST_PASSWORD" type="IMS_PARAMETER" isOptional="true">
                <Description>The DIGEST password to use when chosing DIGEST authentication mode.</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DEREGISTRATION_METHOD" type="DEREGISTRATION_METHOD" isOptional="true">
                <Description>Registration OFF/ON type (None, AIRPLANE_ON_OFF, AT_COMMAND)</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="IP_VERSION" type="IP_VERSION" isOptional="true">
                <Description>IP version used for the APN to connect to the C4I network. IPV4 | IPV6 | IPV4V6</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="AIRPLANE_ON_OFF" type="AIRPLANE_ON_OFF" isOptional="true">
                <Description>Perform airplane ON-OFF and check the IMS registration status</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
