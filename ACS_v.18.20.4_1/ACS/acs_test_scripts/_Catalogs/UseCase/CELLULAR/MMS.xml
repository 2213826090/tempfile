    <UseCase Id="LAB_MMS" Domain="CELLULAR" SubDomain="MMS" OS="ANDROID" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.Communication.Messaging.LAB_MMS.LabMms</ClassName>
        <Description>LAB MMS MO / MT Test Case</Description>
        <Environment name="TEL">
            <Dependency>EQT_AG_8960</Dependency>
        </Environment>
        <Parameters>
            <Parameter name="CELL_TECH" type="CELL_TECHNOLOGY" isOptional="false">
                <!-- Cell Technology (2G|3G) -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_BAND" type="CELL_BAND" isOptional="false">
                <!-- Cell Band. For 2G : EGSM|GSM850|PCS|DCS. For 3G : 1,2,5,8 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_SERVICE" type="CELL_SERVICE" isOptional="false">
                <!-- Cell Service ( (GSM|GPRS|EGPRS) for 2G CELL, (CIRCUIT|PACKET|CIRCUIT_PACKET) for 3G CELL) -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="ARFCN" type="BCH_ARFCN" isOptional="false">
                <!-- Arfcn -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_POWER" type="CELL_POWER_IN_DBM" isOptional="false">
                <!-- cell power -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="LAC" type="LAC" isOptional="false">
                <!-- LAC, For 2G and 3G: 0 to 65535 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="RAC" type="RAC" isOptional="false">
                <!-- Cell Routing Area, For 2G and 3G: 1 to 255 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MCC" type="MCC" isOptional="false">
                <!-- Mobile Country Code 0 to 999 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MNC" type="MNC" isOptional="false">
                <!-- Mobile Network Code 0 to 99 -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_SENDER_NUMBER" type="PHONE_NUMBER" isOptional="false">
                <!-- Sender number. Default value is set to "8960" -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_DESTINATION_NUMBER" type="PHONE_NUMBER" isOptional="false">
                <!-- Destination number. If the value [PHONE_NUMBER] is given, the phoneNumber defined in Phone_Catalog.xml will be used -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_SUBJECT" type="MMS_SUBJECT" isOptional="false">
                <!-- Subject of the MMS in text format -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_TEXT" type="SMS_TEXT" isOptional="false">
                <!-- Text content of the MMS -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_MEDIA" type="MMS_TYPE" isOptional="false">
                <!-- Describes the type of MMS that will be build. Should be "picture" or "text". "picture" for a MMS with subject, text and an attached picture. "text" fot a MMS with subject and text. -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_ATTACHMENT" type="ATTACHMENT_FILE" isOptional="false">
                <!-- Name of a file, present in the multimedia path of the device, that will be attach to the MMS. -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_DIRECTION" type="MOBILE_DIRECTION" isOptional="false">
                <!-- MMS Direction : (MO [Mobile Originated] | MT [Mobile Terminated]) -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MMS_TIMEOUT" type="TIMEOUT" isOptional="false">
                <!-- MMS sending and reception timeout in seconds -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="NOWSMS_USER_NAME" type="USER_NAME" isOptional="false">
                <!-- NowSms Server user name -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="NOWSMS_USER_PASSWORD" type="USER_PASSWORD" isOptional="false">
                <!-- NowSms Server User password -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MOVE_OUT_OF_COVERAGE" type="MOVE_OUT_OF_COVERAGE_SELECTOR" isOptional="true">
                <!-- Description: Optional: Indicates when DUT should move out of coverage, when send or receive MMS. Possible values are: 0 (don't perform loss of coverage), 1 (before sending MMS), 2 (while receiving MMS). -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="OUT_OF_COVERAGE_TIMER" type="TIMEOUT" isOptional="true">
                <!-- Description: Optional: Time (in seconds), DUT should stay in out of coverage state. -->
                <Description>To be filled</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
        </Parameters>
    </UseCase>
