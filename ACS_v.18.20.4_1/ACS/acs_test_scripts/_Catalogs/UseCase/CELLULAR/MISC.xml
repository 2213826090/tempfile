    <UseCase Id="WINTAB_LAB_TURN_OFF_PXT" Domain="MISC" SubDomain="CHECKS" OS="WINDOWS" Status="DONE">
        <ClassName>acs_test_scripts.UseCase.Networking.WINTAB_LAB_TURN_OFF_PXT.LabLteBase</ClassName>
        <Description>check if dut rat matches network rat</Description>
        <Environment name="TEL">
            <Capabilities>EQT_AG_E6621A</Capabilities>
            <Capabilities>EQT_RS_CMW500</Capabilities>
            <Capabilities>CAP_CALLBOX</Capabilities>
        </Environment>
        <Parameters>
            <Parameter name="MIMO" type="STRING" isOptional="true">
                <Description>Multiple-Input Multiple-Output to set : TRUE or FALSE</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="ANTENNAS_NUMBER" type="STRING" isOptional="true">
                <Description>number of antennas to set : 1 or 2</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="TRANSMISSION_MODE" type="STRING" isOptional="true">
                <Description>TRANSMISSION_MODE to set: TM1, TM2, TM3,TM4 ...</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DL_RB_SIZE" type="STRING" isOptional="true">
                <Description>
                    DL RB_SIZE to set: 1.4 MHz = 1 to 6 3 MHz = 1 to 15 5 MHz = 1 to 25 10 MHz = 1 to 50 15 MHz = 1 to 75 20 MHz = 1 to 100
                </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="UL_RB_SIZE" type="STRING" isOptional="true">
                <Description>
                    UL RB_SIZE to set: 1.4 MHz = 1 to 6 3 MHz = 1 to 15 5 MHz = 1 to 25 10 MHz = 1 to 50 15 MHz = 1 to 75 20 MHz = 1 to 100
                </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="TYPE0_BITMAP" type="STRING" isOptional="true">
                <Description>
                    TYPE0_BITMAP to set: requires to set TM3 or TM4 profile, MIMO to be set in scenario file 1.4 MHz = 1 to 63 (0x3F) 3 MHz = 1 to 255 (0xFF) 5 MHz = 1 to 8191 (0x1FFF) 10 MHz = 1 to 131071 (0x1FFFF) 15 MHz = 1 to 524287 (0x7FFFF) 20 MHz = 1 to 33554431 (0x1FFFFFF)
                </Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="UL_GRANT_MODE" type="STRING" isOptional="true">
                <Description>UL GRANT MODE to set: AUTO or FIXEDMAC</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DL_I_MCS" type="STRING" isOptional="true">
                <Description>DL_I_MCS to set: 0 to 28</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="UL_I_MCS" type="STRING" isOptional="true">
                <Description>UL_I_MCS to set: 0 to 31</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_CHANNEL_BANDWIDTH" type="STRING" isOptional="true">
                <Description>Cell Channel Bandwidth in MHz</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="SIGNAL_MODE" type="STRING" isOptional="true">
                <Description>Operation mode to set : BSE or SA</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="PHYSICAL_CELL_ID" type="STRING" isOptional="true">
                <Description>Physical Cell ID : 0 to 503</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_ID" type="STRING" isOptional="true">
                <Description>Cell Setup : "A" for A-cell or "B" for B-cell</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_POWER_RFO1" type="STRING" isOptional="true">
                <Description>Cell power for RFO1</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_POWER_RFO2" type="STRING" isOptional="true">
                <Description>Cell power for RFO2</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MCC" type="STRING" isOptional="true">
                <Description>Mobile Country Code 0 to 999</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="MNC" type="STRING" isOptional="true">
                <Description>Mobile Network Code 0 to 99</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="CELL_BAND" type="STRING" isOptional="true">
                <Description>Cell Band (from Band 1 to Band 32)</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="DL_EARFCN" type="STRING" isOptional="true">
                <Description>Downlink channel</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="SCENARIO_PATH" type="STRING" isOptional="true">
                <Description>Scenario filename to load on PXT</Description>
                <PossibleValues/>
                <DefaultValue/>
                <Blank/>
            </Parameter>
            <Parameter name="PACKET_COUNT" type="STRING" isOptional="true">
