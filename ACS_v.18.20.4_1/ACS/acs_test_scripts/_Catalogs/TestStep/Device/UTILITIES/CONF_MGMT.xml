<?xml version="1.0" encoding="iso-8859-1"?>
<TestSteps xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../acs/src/_Catalogs/TestStep/teststep.xsd">
     <TestStep Id="GET_MODULE_CONFIG" Domain="UTILITIES" SubDomain="CONF_MGMT" Feature="" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.Misc.GetModuleConfig.GetModuleConfig</ClassName>
        <Description>Find a key value in a device module configuration</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device name on which the action will be executed</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="MODULE" type="STRING" isOptional="false">
                    <Description>Device module config to find the key value </Description>
                    <PossibleValues/>
                    <DefaultValue/>
            </Parameter>
            <Parameter name="KEY" type="STRING" isOptional="false">
                <Description> The key value in device module we are looking for</Description>
                <PossibleValues/>
                <DefaultValue/>
            </Parameter>
            <Parameter name="MODULE_VALUE" type="CONTEXT" isOptional="false">
                        <Description>
                             <Key name="MODULE_VALUE_FOUND" type="STRING"> The key value found</Key>
                        </Description>
                        <PossibleValues/>
                        <DefaultValue/>
            </Parameter>
        </Parameters>
    </TestStep>
</TestSteps>
