            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="SET_NTP_STATUS" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.SetNtpStatus.SetNtpStatus</ClassName>
        <Description>Takes a boolean argument. Controls whether NTP based network time synchronization is enabled (if available).</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="NTP_ACTIVE" type="BOOLEAN" isOptional="false">
                <Description>if true, NTP is activated</Description>
                <PossibleValues>true;false</PossibleValues>
                <DefaultValue>true</DefaultValue>
            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="GET_NTP_STATUS" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.GetNtpStatus.GetNtpStatus</ClassName>
        <Description>get the NTP activation status</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="SAVE_STATUS_AS" type="CONTEXT" isOptional="false">
                <Description>
                    <Key name="VALUE" type="STRING">current status of the NTP, True or False</Key>
                </Description>
                <PossibleValues/>
                <DefaultValue/>
            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="SET_RTC_MODE" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.SetRtcMode.SetRtcMode</ClassName>
        <Description>Takes a boolean argument. If "false", the system is configured to maintain the RTC in universal time.
            If "true", it will maintain the RTC in local time instead.</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="RTC_MODE" type="BOOLEAN" isOptional="false">
                <Description>if true, RTC will be set in local time
                if false, RTC will be set in UTC</Description>
                <PossibleValues>true;false;1;0</PossibleValues>
                <DefaultValue>false</DefaultValue>
            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="GET_RTC_MODE" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.GetRtcMode.GetRtcMode</ClassName>
        <Description>get the RTC mode.
        mode "0" is the UTC mode
        mode "1" is the local mode</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="SAVE_MODE_AS" type="CONTEXT" isOptional="false">
                <Description>
                    <Key name="VALUE" type="STRING">current mode of the RTC</Key>
                </Description>
                <PossibleValues/>
                <DefaultValue/>
            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="CHECK_DUT_TIME_VALID" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.CheckTime.CheckTime</ClassName>
        <Description>compare the host time and the DUT time and check he was contain in the gap</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="DATE_REF" type="STRING" isOptional="false">
                <Description>Date to set as format YYYY-MM-DD</Description>
                <PossibleValues/>
                <DefaultValue>2014-10-21</DefaultValue>
            </Parameter>
            <Parameter name="TIME_REF" type="STRING" isOptional="false">
                <Description>Time to set as format hh:mm:ss</Description>
                <PossibleValues/>
                <DefaultValue>00:00:00</DefaultValue>
            </Parameter>
            <Parameter name="GAP_ALLOWED" type="INTEGER" isOptional="false">
                <Description>allowed timeshift between host and dut time in seconds</Description>
                <PossibleValues/>
                <DefaultValue>30</DefaultValue>
            </Parameter>
            <Parameter name="SAVE_AS" type="CONTEXT" isOptional="false">
                <Description>
                    <Key name="VALID" type="BOOLEAN">time is valid or not</Key>
                </Description>
                <PossibleValues/>
                <DefaultValue/>
            </Parameter>
        </Parameters>
    </TestStep>
