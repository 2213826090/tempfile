                <Description>Device name on which the action will be executed</Description>
                <PossibleValues />
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="RESTORATION" type="BOOLEAN" isOptional="false">
                <Description>Set available memory from the backup file when True. In that case MEMORY_TOP will be ignored</Description>
