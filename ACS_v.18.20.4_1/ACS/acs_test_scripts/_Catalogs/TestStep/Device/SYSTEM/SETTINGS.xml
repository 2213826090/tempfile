        <TestStep Id="SET_DATE_TIME" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.SetDateTime.SetDateTime</ClassName>
        <Description>Set the date and the hour of the DUT</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="DATE_FORMAT" type="STRING" isOptional="true">
                <Description>format to return the date with.</Description>
                <PossibleValues/>
                <DefaultValue>%Y-%m-%d</DefaultValue>
            </Parameter>
            <Parameter name="TIME_FORMAT" type="STRING" isOptional="true">
                <Description>format to return the time with.</Description>
                <PossibleValues/>
                <DefaultValue>%H:%M:%S</DefaultValue>
            </Parameter>
            <Parameter name="DATE" type="STRING" isOptional="false">
                <Description>Date to set</Description>
                <PossibleValues/>
                <DefaultValue>2014-10-21</DefaultValue>
            </Parameter>
            <Parameter name="TIME" type="STRING" isOptional="false">
                <Description>Time to set</Description>
                <PossibleValues/>
                <DefaultValue>00:00:00</DefaultValue>
            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="GET_DATE_TIME" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.GetDateTime.GetDateTime</ClassName>
        <Description>Get the date and the hour of the DUT
        if no format specified, DATE is returned with the following format : YYYY-mm-dd
        if no format specified, TIME is returned with the following format : HH:MM:SS</Description>
        <Environment name="ANY"/>
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device that will run the application</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="DATE_FORMAT" type="STRING" isOptional="true">
                <Description>format to return the date with.</Description>
                <PossibleValues/>
                <DefaultValue>%Y-%m-%d</DefaultValue>
            </Parameter>
            <Parameter name="TIME_FORMAT" type="STRING" isOptional="true">
                <Description>format to return the time with.</Description>
                <PossibleValues/>
                <DefaultValue>%H:%M:%S</DefaultValue>
            </Parameter>
            <Parameter name="SAVE_DATE_TIME_AS" type="CONTEXT" isOptional="false">
                <Description>
                    <Key name="DATE" type="STRING">current date of the DUT</Key>
                    <Key name="TIME" type="STRING">current time of the DUT</Key>
                </Description>
                <PossibleValues/>
                <DefaultValue/>
            </Parameter>
        </Parameters>
    </TestStep>
    <TestStep Id="SET_TIMEZONE" Domain="SYSTEM" SubDomain="SETTINGS" Feature="COMMON" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.System.Settings.SetTimezone.SetTimezone</ClassName>
        <Description>Set the system time zone to the specified value.</Description>
