        <TestStep Id="SET_DEVICE_APN" Domain="WIRELESS" SubDomain="COMMON" Feature="IP_MGMT" Status="DONE">
        <ClassName>acs_test_scripts.TestStep.Device.Wireless.Common.IPMgmt.SetDeviceApn.SetDeviceApn</ClassName>
        <Description>Sets an APN for the designated device</Description>
        <Environment name="ANY" />
        <Parameters>
            <Parameter name="DEVICE" type="STRING" isOptional="false">
                <Description>Device name on which the action will be executed</Description>
                <PossibleValues/>
                <DefaultValue>PHONE1</DefaultValue>
            </Parameter>
            <Parameter name="INTERFACE" type="STRING" isOptional="false">
                <Description>Data Cellular interface. Set to 'None' to use the default APN interface</Description>
                <PossibleValues/>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="APN" type="STRING" isOptional="false">
                <Description>Cellular Access Point Name. Set to 'None' to use the default APN name</Description>
                <PossibleValues/>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="USER" type="STRING" isOptional="false">
                <Description>The APN user's login. If no user's login, set to 'None'</Description>
                <PossibleValues/>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="PASSWORD" type="STRING" isOptional="false">
                <Description>The APN user's password</Description>
                <PossibleValues/>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="PROTOCOL" type="STRING" isOptional="false">
                <Description>The APN protocol ("IP", "IPV6", "IPV4V6"). Setting to 'None' will use the default value (IP)</Description>
                <PossibleValues>None;IP;IPV6;IPV4V6</PossibleValues>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="MMSC" type="STRING" isOptional="false">
                <Description>The APN MMSC (ex: http://10.102.161.47:8080/8960=8960). If there is no APN MMSC, set to 'None'</Description>
                <PossibleValues/>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="APN_TYPE" type="STRING" isOptional="false">
                <Description>The APN Type (ex : default,mms). If no specific type, set to 'None'</Description>
                <PossibleValues/>
                <DefaultValue>None</DefaultValue>
            </Parameter>
            <Parameter name="SET_DEFAULT" type="BOOLEAN" isOptional="false">
                <Description>Boolean determining if new APN is set as default APN</Description>
                <PossibleValues>True;False</PossibleValues>
                <DefaultValue>True</DefaultValue>
            </Parameter>
            <Parameter name="CLEAR_APN" type="BOOLEAN" isOptional="false">
                <Description>Boolean determining if existing APN(s) is/are kept or removed</Description>
                <PossibleValues>True;False</PossibleValues>
                <DefaultValue>True</DefaultValue>
            </Parameter>
        </Parameters>
    </TestStep>
